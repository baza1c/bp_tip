		<query>ВЫБРАТЬ
	&amp;Показатель КАК Показатель,
	ХозрасчетныйОбороты.Период КАК Период,
	ХозрасчетныйОбороты.Организация КАК Организация,
	ХозрасчетныйОбороты.Подразделение КАК Подразделение,
	ХозрасчетныйОбороты.Счет КАК Счет,
	ХозрасчетныйОбороты.КорСчет КАК КорСчет,
	ХозрасчетныйОбороты.Регистратор КАК Документ,
	ВЫБОР
		КОГДА ХозрасчетныйОбороты.СуммаОборот &lt; 0
				И -ХозрасчетныйОбороты.СуммаОборот = ХозрасчетныйОбороты.СуммаОборотКт
			ТОГДА ХозрасчетныйОбороты.СуммаОборотКт
		ИНАЧЕ ХозрасчетныйОбороты.СуммаОборотДт
	КОНЕЦ КАК Сумма,
	"" КАК Содержание,
	ХозрасчетныйОбороты.КорСубконто1 КАК КорСубконто1,
	ХозрасчетныйОбороты.КорСубконто2 КАК КорСубконто2,
	ХозрасчетныйОбороты.КорСубконто3 КАК КорСубконто3,
	ХозрасчетныйОбороты.Субконто1 КАК Субконто1,
	ХозрасчетныйОбороты.Субконто2 КАК Субконто2,
	ХозрасчетныйОбороты.Субконто3 КАК Субконто3
{ВЫБРАТЬ
	Период,
	Организация.*,
	Подразделение.*,
	Документ.*,
	Сумма,
	Содержание,
	Показатель,
	Субконто1.*,
	Субконто2.*,
	Субконто3.*,
	КорСубконто1.*,
	КорСубконто2.*,
	КорСубконто3.*}
ИЗ
	РегистрБухгалтерии.Хозрасчетный.Обороты(
			&amp;НачалоПериода,
			&amp;КонецПериода,
			Регистратор,
			Счет В (&amp;Счет),
			,
			&amp;УсловиеКорСубконто
				И &amp;УсловиеСубконто
				И (Организация = &amp;Организация
					ИЛИ &amp;ПоВсемОрганизациям),
			КорСчет В (&amp;КорСчет),
			) КАК ХозрасчетныйОбороты
{ГДЕ
	ХозрасчетныйОбороты.Организация.*,
	ХозрасчетныйОбороты.Подразделение.*,
	ХозрасчетныйОбороты.Регистратор.* КАК Документ}

УПОРЯДОЧИТЬ ПО
	Период</query>
