	ХозрасчетныйОстатки.Организация КАК Организация,
	ХозрасчетныйОстатки.Подразделение КАК Подразделение,
	ХозрасчетныйОстатки.РасчетныйСчет КАК РасчетныйСчет,
	ХозрасчетныйОстатки.Валюта КАК Валюта,
	МИНИМУМ(ХозрасчетныйОстатки.Период) КАК Период,
	ДниКалендаря.ДатаКалендаря КАК ДатаКалендаря
ПОМЕСТИТЬ ТаблицаПериодов
ИЗ
	ДниКалендаря КАК ДниКалендаря
		ЛЕВОЕ СОЕДИНЕНИЕ ХозрасчетныйОстатки КАК ХозрасчетныйОстатки
		ПО ДниКалендаря.ДатаКалендаря &lt;= ХозрасчетныйОстатки.Период

СГРУППИРОВАТЬ ПО
	ХозрасчетныйОстатки.Организация,
	ХозрасчетныйОстатки.Подразделение,
	ХозрасчетныйОстатки.РасчетныйСчет,
	ХозрасчетныйОстатки.Валюта,
	ДниКалендаря.ДатаКалендаря
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ХозрасчетныйОстатки.Организация КАК Организация,
	ХозрасчетныйОстатки.Подразделение КАК Подразделение,
	ХозрасчетныйОстатки.РасчетныйСчет КАК РасчетныйСчет,
	ХозрасчетныйОстатки.Валюта КАК Валюта,
	СУММА(ХозрасчетныйОстатки.СуммаНачальныйОстаток) КАК СуммаНачальныйОстаток,
	СУММА(ХозрасчетныйОстатки.ВалютнаяСуммаНачальныйОстаток) КАК ВалютнаяСуммаНачальныйОстаток,
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ ТаблицаПериодов.ДатаКалендаря) КАК Период
ПОМЕСТИТЬ Остатки
{ВЫБРАТЬ
	Организация.*,
	Подразделение.*,
	РасчетныйСчет,
	Валюта.*}
ИЗ
	ТаблицаПериодов КАК ТаблицаПериодов
		ЛЕВОЕ СОЕДИНЕНИЕ ХозрасчетныйОстатки КАК ХозрасчетныйОстатки
		ПО ТаблицаПериодов.Период = ХозрасчетныйОстатки.Период
			И ТаблицаПериодов.Организация = ХозрасчетныйОстатки.Организация
			И ТаблицаПериодов.Подразделение = ХозрасчетныйОстатки.Подразделение
			И ТаблицаПериодов.РасчетныйСчет = ХозрасчетныйОстатки.РасчетныйСчет
			И ТаблицаПериодов.Валюта = ХозрасчетныйОстатки.Валюта

СГРУППИРОВАТЬ ПО
	ХозрасчетныйОстатки.Организация,
	ХозрасчетныйОстатки.Подразделение,
	ХозрасчетныйОстатки.РасчетныйСчет,
	ХозрасчетныйОстатки.Валюта
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Остатки.Организация КАК Организация,
	Остатки.Подразделение КАК Подразделение,
	"СреднийОстаток" КАК ВидОперации,
	Остатки.РасчетныйСчет КАК РасчетныйСчет,
	Остатки.РасчетныйСчет.Банк КАК Банк,
	Остатки.Валюта КАК Валюта,
	ВЫБОР
		КОГДА Остатки.Валюта = НЕОПРЕДЕЛЕНО
			ТОГДА Остатки.СуммаНачальныйОстаток / Остатки.Период
		ИНАЧЕ 0
	КОНЕЦ КАК Сумма,
	Остатки.ВалютнаяСуммаНачальныйОстаток / Остатки.Период КАК ВалютнаяСумма,
	1 КАК Порядок,
	"СреднийОстаток" КАК СреднийОстаток
ИЗ
	Остатки КАК Остатки
{ГДЕ
	Остатки.Организация.*,
	Остатки.Подразделение.*,
	("СреднийОстаток") КАК ВидОперации,
	Остатки.РасчетныйСчет,
	Остатки.РасчетныйСчет.Банк КАК Банк,
	Остатки.Валюта.*,
	(1) КАК Порядок}</query>
		</item>
	</dataSet>
