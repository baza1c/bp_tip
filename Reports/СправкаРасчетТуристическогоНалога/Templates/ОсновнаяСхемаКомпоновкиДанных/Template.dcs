			И ВТ_ИтоговыеЗначения.Ставка = ВТ_СтоимостиПоСтавкеСОкруглением.Ставка
ГДЕ
	(ВТ_ИтоговыеЗначения.СуммаНалогаПогрешность &lt;&gt; 0
			ИЛИ (ВЫРАЗИТЬ(ВТ_ИтоговыеЗначения.СтоимостьУслуг КАК ЧИСЛО(15, 0))) - ВТ_ИтоговыеЗначения.СтоимостьУслуг &lt;&gt; 0
			ИЛИ (ВЫРАЗИТЬ(ВТ_ИтоговыеЗначения.СтоимостьУслугБезНалога КАК ЧИСЛО(15, 0))) - ВТ_ИтоговыеЗначения.СтоимостьУслугБезНалога &lt;&gt; 0)</query>
