	<totalField>
		<dataPath>Отклонение</dataPath>
		<expression>ВЫБОР КОГДА  Показатель = "Рентабельность продаж" 
ТОГДА 0
ИНАЧЕ
		
		ВЫБОР КОГДА &amp;Периодичность = 0 ТОГДА
			
			ВЫБОР
				КОГДА ВычислитьВыражение("Год","Год", , "Текущая", "Текущая") = &amp;НачальныйПериод
				ТОГДА 0
				КОГДА ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Год", , "Текущая", "Текущая") = 0 И
					  ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Год",, "Предыдущая", "Предыдущая") = 0
				ТОГДА 0
				КОГДА ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Год", , "Текущая", "Текущая") = 0 И
					  ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Год",, "Предыдущая", "Предыдущая") &lt;&gt; 0
				ТОГДА 0
				КОГДА ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Год", , "Текущая", "Текущая") &lt;&gt; 0 И
					  ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Год",, "Предыдущая", "Предыдущая") = 0
				ТОГДА 0		
				
				ИНАЧЕ (ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Год", , "Предыдущая", "Предыдущая")
				/ ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Год",, "Текущая", "Текущая") - 1) * 100
				* ( ВЫБОР КОГДА  
					ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Год",, "Текущая", "Текущая") &lt; 0 ТОГДА -1 ИНАЧЕ 1 КОНЕЦ)
			КОНЕЦ
			
		ИНАЧЕ		
		
			ВЫБОР
			КОГДА ВычислитьВыражение("Месяц","Месяц", , "Текущая", "Текущая") = &amp;НачальныйПериод
			ТОГДА 0
			КОГДА ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Месяц", , "Текущая", "Текущая") = 0 И
				ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Месяц",, "Предыдущая(1)", "Предыдущая(1)") = 0
			ТОГДА 0
			КОГДА ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Месяц", , "Текущая", "Текущая") = 0 И
				ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Месяц",, "Предыдущая(1)", "Предыдущая(1)") &lt;&gt; 0
			ТОГДА 0
			КОГДА ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Месяц", , "Текущая", "Текущая") &lt;&gt; 0 И
				ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Месяц",, "Предыдущая(1)", "Предыдущая(1)") = 0
			ТОГДА 0
			ИНАЧЕ ((ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Месяц",, "Текущая", "Текущая"))
				/ ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Месяц", , "Предыдущая(1)", "Предыдущая(1)") - 1) * 100
				* ( ВЫБОР КОГДА  
					ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Месяц", , "Предыдущая(1)", "Предыдущая(1)") &lt; 0 Тогда -1 ИНАЧЕ 1 КОНЕЦ)
			КОНЕЦ
		
		КОНЕЦ
КОНЕЦ
</expression>
