    </notDefaultUseAlwaysAttributes>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ РАЗЛИЧНЫЕ
	ЖурналОпераций.Дата КАК Дата,
	ЖурналОпераций.Номер КАК Номер,
	ЖурналОпераций.Тип КАК ВидДокумента,
	ЖурналОпераций.Информация КАК Информация,
	ЖурналОпераций.СуммаДокумента КАК СуммаДокумента,
	ЖурналОпераций.ВалютаДокумента КАК ВалютаДокумента,
	ЖурналОпераций.ВидОперации КАК ВидОперации,
	ЖурналОпераций.Ссылка КАК Ссылка,
	ЖурналОпераций.НомерВходящегоДокумента КАК НомерВходящегоДокумента,
	ЖурналОпераций.ДатаВходящегоДокумента КАК ДатаВходящегоДокумента,
	ВЫБОР
		КОГДА ЖурналОпераций.СостояниеРеглОперации ЕСТЬ NULL
			ТОГДА ВЫБОР
					КОГДА ЖурналОпераций.Тип = ТИП(Документ.ОперацияБух)
						ТОГДА ВЫБОР
								КОГДА ЖурналОпераций.ПометкаУдаления = ИСТИНА
									ТОГДА 2
								ИНАЧЕ 1
							КОНЕЦ
					ИНАЧЕ ВЫБОР
							КОГДА ЖурналОпераций.РучнаяКорректировка = ИСТИНА
								ТОГДА ВЫБОР
										КОГДА ЖурналОпераций.ПометкаУдаления = ИСТИНА
											ТОГДА 10
										КОГДА ЖурналОпераций.Проведен = ЛОЖЬ
											ТОГДА 9
										ИНАЧЕ 8
									КОНЕЦ
							ИНАЧЕ ВЫБОР
									КОГДА ЖурналОпераций.ПометкаУдаления = ИСТИНА
										ТОГДА 2
									КОГДА ЖурналОпераций.Проведен = ИСТИНА
										ТОГДА 1
									ИНАЧЕ 0
								КОНЕЦ
						КОНЕЦ
				КОНЕЦ
		ИНАЧЕ ВЫБОР
				КОГДА ЖурналОпераций.ПометкаУдаления
					ТОГДА 2
				КОГДА ЖурналОпераций.СостояниеРеглОперации = ЗНАЧЕНИЕ(Перечисление.ВидыСостоянийРегламентныхОпераций.НеВыполнено)
						ИЛИ ЖурналОпераций.СостояниеРеглОперации = ЗНАЧЕНИЕ(Перечисление.ВидыСостоянийРегламентныхОпераций.Выполняется)
					ТОГДА 0
				КОГДА ЖурналОпераций.СостояниеРеглОперации = ЗНАЧЕНИЕ(Перечисление.ВидыСостоянийРегламентныхОпераций.ВыполненоСошибками)
					ТОГДА 6
				КОГДА ЖурналОпераций.СостояниеРеглОперации = ЗНАЧЕНИЕ(Перечисление.ВидыСостоянийРегламентныхОпераций.Выполнено)
						И ЖурналОпераций.РучнаяКорректировка
					ТОГДА 5
				КОГДА ЖурналОпераций.СостояниеРеглОперации = ЗНАЧЕНИЕ(Перечисление.ВидыСостоянийРегламентныхОпераций.Пропущено)
					ТОГДА 7
				ИНАЧЕ 4
			КОНЕЦ
	КОНЕЦ КАК СостояниеДокумента,
	ЖурналОпераций.Организация КАК Организация
{ВЫБРАТЬ
	Дата,
	Номер,
	ВидДокумента,
	Информация.*,
	СуммаДокумента,
	ВалютаДокумента,
	ВидОперации,
	Ссылка.*,
	НомерВходящегоДокумента,
	ДатаВходящегоДокумента,
	СостояниеДокумента}
ИЗ
	ЖурналДокументов.ЖурналОпераций КАК ЖурналОпераций
		{ЛЕВОЕ СОЕДИНЕНИЕ РегистрБухгалтерии.Хозрасчетный.ДвиженияССубконто КАК ХозрасчетныйДвиженияССубконтоДт
		ПО ЖурналОпераций.Ссылка = ХозрасчетныйДвиженияССубконтоДт.Регистратор}
		{ЛЕВОЕ СОЕДИНЕНИЕ РегистрБухгалтерии.Хозрасчетный.ДвиженияССубконто КАК ХозрасчетныйДвиженияССубконтоКт
		ПО ЖурналОпераций.Ссылка = ХозрасчетныйДвиженияССубконтоКт.Регистратор}
ГДЕ
	НЕ ЖурналОпераций.ПометкаУдаления
{ГДЕ
	ХозрасчетныйДвиженияССубконтоДт.СчетДт.*,
	ХозрасчетныйДвиженияССубконтоДт.СубконтоДт1.*,
	ХозрасчетныйДвиженияССубконтоДт.СубконтоДт2.*,
	ХозрасчетныйДвиженияССубконтоДт.СубконтоДт3.*,
	ХозрасчетныйДвиженияССубконтоКт.СчетКт.*,
	ХозрасчетныйДвиженияССубконтоКт.СубконтоКт1.*,
	ХозрасчетныйДвиженияССубконтоКт.СубконтоКт2.*,
	ХозрасчетныйДвиженияССубконтоКт.СубконтоКт3.*,
	ЖурналОпераций.Информация.*,
	ЖурналОпераций.ДоговорКонтрагента.*,
	ЖурналОпераций.Номер,
	ЖурналОпераций.Дата,
	ЖурналОпераций.Ссылка.*,
	(ЖурналОпераций.Тип В (&amp;ВидыДокументов)),
	(ЖурналОпераций.Дата >= &amp;НачалоПериода),
	(ЖурналОпераций.Дата &lt;= &amp;КонецПериода),
	(ЖурналОпераций.Организация = &amp;Организация),
	ЖурналОпераций.НомерВходящегоДокумента,
	ЖурналОпераций.ДатаВходящегоДокумента,
	ЖурналОпераций.СуммаДокумента,
	ЖурналОпераций.ВалютаДокумента.*,
	ЖурналОпераций.Тип КАК ВидДокумента}</queryText>
