		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	Хозрасчетный.Ссылка КАК Счет,
	НЕ ВидСубконтоДоговоры.ВидСубконто ЕСТЬ NULL КАК ЕстьСубконтоДоговоры,
	ИСТИНА КАК СчетРасчетовСПоставщиками,
	ЛОЖЬ КАК СчетРасчетовСПокупателями
ПОМЕСТИТЬ СчетаРасчетов
ИЗ
	ПланСчетов.Хозрасчетный КАК Хозрасчетный
		ЛЕВОЕ СОЕДИНЕНИЕ ПланСчетов.Хозрасчетный.ВидыСубконто КАК ВидСубконтоДоговоры
		ПО Хозрасчетный.Ссылка = ВидСубконтоДоговоры.Ссылка
			И (ВидСубконтоДоговоры.ВидСубконто = &amp;СубконтоДоговоры)
{ГДЕ
	Хозрасчетный.Ссылка КАК СчетаРасчетовСПоставщиками,
	(ИСТИНА) КАК Поставщики}

ОБЪЕДИНИТЬ

ВЫБРАТЬ РАЗЛИЧНЫЕ
	Хозрасчетный.Ссылка,
	НЕ ВидСубконтоДоговоры.ВидСубконто ЕСТЬ NULL,
	ЛОЖЬ,
	ИСТИНА
ИЗ
	ПланСчетов.Хозрасчетный КАК Хозрасчетный
		ЛЕВОЕ СОЕДИНЕНИЕ ПланСчетов.Хозрасчетный.ВидыСубконто КАК ВидСубконтоДоговоры
		ПО Хозрасчетный.Ссылка = ВидСубконтоДоговоры.Ссылка
			И (ВидСубконтоДоговоры.ВидСубконто = &amp;СубконтоДоговоры)
{ГДЕ
	Хозрасчетный.Ссылка КАК СчетаРасчетовСПокупателями,
	(ИСТИНА) КАК Покупатели}

ИНДЕКСИРОВАТЬ ПО
	Счет
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Остатки.Субконто1 КАК Контрагент,
	Остатки.Счет КАК Счет,
	СУММА(Остатки.СуммаОстатокДт) КАК ДебиторскаяЗадолженность,
	СУММА(Остатки.СуммаОстатокКт) КАК КредиторскаяЗадолженность
ПОМЕСТИТЬ ОстаткиРасчетов
ИЗ
	РегистрБухгалтерии.Хозрасчетный.Остатки(
			{(&amp;Период)},
			Счет В
				(ВЫБРАТЬ
					СчетаРасчетов.Счет
				ИЗ
					СчетаРасчетов),
			&amp;СубконтоКонтрагенты,
			{(Организация) КАК Организация, (Субконто1).* КАК Контрагент}) КАК Остатки

СГРУППИРОВАТЬ ПО
	Остатки.Субконто1,
	Остатки.Счет
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	ДатыОпераций.Контрагент КАК Контрагент,
	ДатыОпераций.Договор КАК Договор,
	ДатыОпераций.Период КАК Период,
	ДатыОпераций.Счет КАК Счет
ПОМЕСТИТЬ ОперацииЗаПериод
ИЗ
	(ВЫБРАТЬ
		Обороты.Субконто1 КАК Контрагент,
		Обороты.Счет КАК Счет,
		ЗНАЧЕНИЕ(Справочник.ДоговорыКонтрагентов.ПустаяСсылка) КАК Договор,
		Обороты.Период КАК Период
	ИЗ
		РегистрБухгалтерии.Хозрасчетный.Обороты(
				{(&amp;НачалоПериода)},
				{(&amp;КонецПериода)},
				Регистратор,
				Счет В
					(ВЫБРАТЬ
						СчетаРасчетов.Счет
					ИЗ
						СчетаРасчетов
					ГДЕ
						НЕ СчетаРасчетов.ЕстьСубконтоДоговоры),
				&amp;СубконтоКонтрагенты,
				{(Организация) КАК Организация, (Субконто1).* КАК Контрагент},
				,
				) КАК Обороты
	
	ОБЪЕДИНИТЬ ВСЕ
	
	ВЫБРАТЬ
		Обороты.Субконто1,
		Обороты.Счет,
		Обороты.Субконто2,
		Обороты.Период
	ИЗ
		РегистрБухгалтерии.Хозрасчетный.Обороты(
				{(&amp;НачалоПериода)},
				{(&amp;КонецПериода)},
				Регистратор,
				Счет В
					(ВЫБРАТЬ
						СчетаРасчетов.Счет
					ИЗ
						СчетаРасчетов
					ГДЕ
						СчетаРасчетов.ЕстьСубконтоДоговоры),
				&amp;СубконтоКонтрагентыДоговоры,
				{(Организация) КАК Организация, (Субконто1).* КАК Контрагент},
				,
				) КАК Обороты) КАК ДатыОпераций

ИНДЕКСИРОВАТЬ ПО
	Контрагент,
	Договор
;

