
&НаКлиенте
Процедура ОбработкаКоманды(ПараметрКоманды, ПараметрыВыполненияКоманды)
	
	ПараметрыФормы = Новый Структура;
	Параметрыформы.Вставить("ВариантОтчета",           "ВедомостьПоЕНС");
	Параметрыформы.Вставить("НачалоПериода",           НачалоГода(ТекущаяДата()));
	Параметрыформы.Вставить("КонецПериода",            КонецДня(ТекущаяДата()));
	ПараметрыФормы.Вставить("СформироватьПриОткрытии", Истина);
	
	ОткрытьФорму("Отчет.РасчетыПоЕНС.Форма.ФормаОтчета", ПараметрыФормы);
	
КонецПроцедуры
