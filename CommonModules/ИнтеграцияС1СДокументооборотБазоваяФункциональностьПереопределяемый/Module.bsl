//			Объект.Состояние = Перечисления._ДемоСостоянияЗаказаПокупателя.Подготовлен;
//		КонецЕсли;
//		Объект.Записать(РежимЗаписиДокумента.Запись);
//	
//	КонецЕсли;
//
Процедура ПриИзмененииСостоянияСогласования(ПредметСогласования, Состояние, ВызовИзФормыОбъекта, Установил,
		ДатаУстановки) Экспорт
	
	СостояниеЗаявкиУХ = "";
	Если Состояние = Перечисления.СостоянияСогласованияВДокументообороте.НаСогласовании Тогда
		СостояниеЗаявкиУХ = "НаУтверждении";
	ИначеЕсли Состояние = Перечисления.СостоянияСогласованияВДокументообороте.НеСогласован Тогда
		СостояниеЗаявкиУХ = "Отклонена";
	ИначеЕсли Состояние = Перечисления.СостоянияСогласованияВДокументообороте.Согласован Тогда
		СостояниеЗаявкиУХ = "Утверждена";
	КонецЕсли;
	
	Если ОбщегоНазначения.ПодсистемаСуществует("ПодсистемыУХ") И ЗначениеЗаполнено(СостояниеЗаявкиУХ) Тогда
		ИмяПеречисления = "СостоянияСогласования";
		ИмяРегистра = "РегистрСостоянийОбъектов";
		ТипыЗаявок = Метаданные.РегистрыСведений[ИмяРегистра].Измерения.Объект.Тип.Типы();
		Если ТипыЗаявок.Найти(ТипЗнч(ПредметСогласования)) <> Неопределено Тогда
			МодульУправлениеПроцессамиСогласованияУХ = ОбщегоНазначения.ОбщийМодуль("УправлениеПроцессамиСогласованияУХ");
			Попытка
				МодульУправлениеПроцессамиСогласованияУХ.ПеревестиЗаявкуВПроизвольноеСостояние(
					ПредметСогласования,
					Перечисления[ИмяПеречисления][СостояниеЗаявкиУХ]);
			Исключение
				ИнтеграцияС1СДокументооборотБазоваяФункциональность.ЗаписатьОшибку(
					ОбработкаОшибок.ПодробноеПредставлениеОшибки(ИнформацияОбОшибке()));
			КонецПопытки;
		КонецЕсли;
	КонецЕсли;
	
КонецПроцедуры

