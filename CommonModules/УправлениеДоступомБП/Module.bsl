	НоваяСтрока = РолиПросмотраРедактирования.Добавить();
	НоваяСтрока.РольПросмотра      = "ЧтениеНеразделяемыхДанныхОбменаСБанками";
	НоваяСтрока.РольРедактирования = "ДобавлениеИзменениеНеразделяемыхДанныхОбменаСБанками";
	НоваяСтрока.ОбщиеДанные        = Истина;
	
	// Отдельной роли для чтения общих (неразделяемых) данных регламентированной отчетности в БРО нет.
	// Права на чтение этих данных предоставляются одновременно с правами на чтение разделяемых данных.
	НоваяСтрока = РолиПросмотраРедактирования.Добавить();
	НоваяСтрока.РольПросмотра      = "ЧтениеДанныхРегламентированнойОтчетности";
	НоваяСтрока.РольРедактирования = "ДобавлениеИзменениеНеразделяемыхДанныхРегламентированнойОтчетности";
	НоваяСтрока.ОбщиеДанные        = Истина;
	
	Возврат РолиПросмотраРедактирования;
	
КонецФункции

Процедура ВытеснитьРолиИнтегральнымиРолями(ОписанияПрофилей)
	
	ИнтегральныеРоли = ИнтегральныеРолиВсеДанныеБухгалтерии();
	ДополнитьИнтегральныеРолиДобавлениеИзменениеБанковскихСчетов(ИнтегральныеРоли);
	
	// Чтобы упростить описание ролей, атомарные роли включаются в профили там, где это уместно.
	// Вместе с тем, не следует давать одному пользователю атомарную и интегральную роль,
	// предоставляющие права к одному и тому же объекту.
	// Поэтому из профиля, содержащего интегральную роль, исключим "вытесненные" им атомарные.
	
	Для Каждого ОписаниеПрофиля Из ОписанияПрофилей Цикл
		
		Для Каждого ОписаниеИнтегральнойРоли Из ИнтегральныеРоли Цикл
			
			Если ОписаниеПрофиля.Роли.Найти(ОписаниеИнтегральнойРоли.Ключ) = Неопределено Тогда
				Продолжить; // Интегральная роль не назначена
			КонецЕсли;
			
			Для Каждого ИмяВытесненнойРоли Из ОписаниеИнтегральнойРоли.Значение Цикл
				
				ИндексВытесненнойРоли = ОписаниеПрофиля.Роли.Найти(ИмяВытесненнойРоли);
				Если ИндексВытесненнойРоли = Неопределено Тогда
					Продолжить;
				КонецЕсли;
				
				ОписаниеПрофиля.Роли.Удалить(ИндексВытесненнойРоли);
				
			КонецЦикла;
			
		КонецЦикла;
		
	КонецЦикла;
	
КонецПроцедуры

Процедура ИсключитьИзбыточныеРолиПросмотра(ОписанияПрофилей)
	
	// Иногда при описании профиля удобнее, понятнее предоставить и право просмотра (скажем, в составе общих прав для нескольких профилей)
	// и право редактирования - как дополнительное право конкретного профиля.
	// Однако, с точки зрения исполнения профиля это избыточно.
	// Поэтому избыточные роли просмотра следует избегать там, где это возможно.
	
	РолиПросмотраРедактирования = РолиПросмотраРедактирования();
	
	Для Каждого ОписаниеПрофиля Из ОписанияПрофилей Цикл
		
		Для Каждого ПараРолейПросмотрРедактирование Из РолиПросмотраРедактирования Цикл
			
			РольРедактирования = ОписаниеПрофиля.Роли.Найти(ПараРолейПросмотрРедактирование.РольРедактирования);
			Если РольРедактирования = Неопределено Тогда
				Продолжить;
			КонецЕсли;
			
			РольПросмотра = ОписаниеПрофиля.Роли.Найти(ПараРолейПросмотрРедактирование.РольПросмотра);
			Если РольПросмотра = Неопределено Тогда
				Продолжить;
			КонецЕсли;
			
			// Есть одновременно роль просмотра и редактирования.
			// Роль просмотра - избыточная.
			ОписаниеПрофиля.Роли.Удалить(РольПросмотра);
			
		КонецЦикла;
		
	КонецЦикла;
	
КонецПроцедуры

Процедура ЗапретитьРедактированиеОбщихДанных(ОписанияПрофилей)
	
	// Если в информационной базе используются разделители, то только администратор базы может редактировать общие данные.
	// У прикладных пользователей редактирование следует запретить, предоставив права на чтение.
	
	Если Не ОбщегоНазначения.РазделениеВключено() Тогда
		Возврат;
	КонецЕсли;
	
	РолиПросмотраРедактирования = РолиПросмотраРедактирования();
	
	Для Каждого ОписаниеПрофиля Из ОписанияПрофилей Цикл
		
		Для Каждого ПараРолейПросмотрРедактирование Из РолиПросмотраРедактирования Цикл
			
			Если Не ПараРолейПросмотрРедактирование.ОбщиеДанные Тогда
				Продолжить;
			КонецЕсли;
			
			РольРедактирования = ОписаниеПрофиля.Роли.Найти(ПараРолейПросмотрРедактирование.РольРедактирования);
			Если РольРедактирования = Неопределено Тогда
				Продолжить;
			КонецЕсли;
			
			// Эту роль следует исключить из профиля, предоставив взамен роль на чтение этих данных.
			ОписаниеПрофиля.Роли.Удалить(РольРедактирования);
			
			РольПросмотра = ОписаниеПрофиля.Роли.Найти(ПараРолейПросмотрРедактирование.РольПросмотра);
			Если РольПросмотра <> Неопределено Тогда
				// Роль просмотра уже есть в профиле, добавлять ее не надо
				Продолжить;
			КонецЕсли;
			
			ОписаниеПрофиля.Роли.Добавить(ПараРолейПросмотрРедактирование.РольПросмотра);
			
		КонецЦикла;
		
	КонецЦикла;
	
КонецПроцедуры

Процедура ДополнитьИнтегральныеРолиДобавлениеИзменениеБанковскихСчетов(ИнтегральныеРоли)
	
	// Справочник БанковскиеСчета подчинен разным владельцам - Организации и Контрагенты - права на редактирование которых
	// включены в разные роли.
	
	// В конфигурации реализована роль ДобавлениеИзменениеБанковскихСчетов, предоставляющая право редактирования счетов,
	// подчиненных только тем владельцам, к редактированию которых у пользователя есть доступ.
	
	// При этом для отдельных пользователей важно предоставить доступ к редактированию банковских счетов организаций,
	// несмотря на то, что права на редактирование организаций у них нет.
	
	// Поэтому право редактирования банковских счетов включено в роль ДобавлениеИзменениеСведенийОбОрганизациях.
	
	// Таким образом, интегральная роль ДобавлениеИзменениеСведенийОбОрганизациях включает права,
	// предоставляемые ролью ДобавлениеИзменениеБанковскихСчетов, поэтому одновременно две роли предоставлять не следует.
	
	ИнтегральныеРоли.Вставить("ДобавлениеИзменениеСведенийОбОрганизациях", Новый Массив);
	ВытесняемыеРоли = ИнтегральныеРоли.ДобавлениеИзменениеСведенийОбОрганизациях;
	ВытесняемыеРоли.Добавить("ДобавлениеИзменениеБанковскихСчетов");
	
КонецПроцедуры

#КонецОбласти

