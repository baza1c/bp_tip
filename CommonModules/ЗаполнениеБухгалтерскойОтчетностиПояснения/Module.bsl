	Контекст = Новый Структура;
	
	Контекст.Вставить("НачалоПериода", '0001-01-01');
	Контекст.Вставить("КонецПериода",  '0001-01-01');
	Контекст.Вставить("Организация",   Справочники.Организации.ПустаяСсылка());
	
	Возврат Контекст;
	
