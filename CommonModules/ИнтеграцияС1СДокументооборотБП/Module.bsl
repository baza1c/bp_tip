#Область СлужебныеПроцедурыИФункции

Функция СоздатьПравилоИнтеграцииПоступлениеТоваровУслуг(ВидОперации, ВидДокумента, ТипДокумента)
	
	ПравилаИнтеграцииМенеджер = Справочники.ПравилаИнтеграцииС1СДокументооборотом;
	ШаблоныВыражений =
		ИнтеграцияС1СДокументооборотБазоваяФункциональностьВызовСервера.ШаблоныВыраженийНаВстроенномЯзыке();
	
	Правило = Справочники.ПравилаИнтеграцииС1СДокументооборотом.СоздатьЭлемент();
	Правило.Заполнить(Неопределено);
	Правило.ТипОбъектаИС = "Документ.ПоступлениеТоваровУслуг";
	Правило.ТипОбъектаДО = ТипДокумента;
	Правило.Комментарий = "Создано автоматически";
	
	// Ключевые реквизиты, вид операции и вид документа.
	
	// вид операции
	ПравилоЗаполнения = ПравилаИнтеграцииМенеджер.НовоеПравилоЗаполненияРеквизита(ВидОперации);
	ПравилоЗаполнения.ОбновлятьИС = Ложь;
	ПравилоЗаполнения.Ключевой = Истина;
	ПравилоЗаполнения.РежимИзменения = Перечисления.РежимИзмененияПроведенногоДокументаДанными1СДокументооборота.Запрещено;
	ПравилаИнтеграцииМенеджер.ДобавитьПравилоЗаполненияРеквизитаИС(
		Правило,
		Перечисления.ВариантыПравилЗаполненияРеквизитов.УказанноеЗначение,
		"ВидОперации",
		ПравилоЗаполнения);
	
	// вид документа
	ПравилоЗаполнения = ПравилаИнтеграцииМенеджер.НовоеПравилоЗаполненияРеквизита(ВидДокумента);
	ПравилоЗаполнения.ОбновлятьДО = Ложь;
	ПравилоЗаполнения.Ключевой = Истина;
	ПравилаИнтеграцииМенеджер.ДобавитьПравилоЗаполненияРеквизитаДО(
		Правило,
		Перечисления.ВариантыПравилЗаполненияРеквизитов.УказанноеЗначение,
		"documentType",
		ПравилоЗаполнения);
	
	// Парные правила.
	
	ПравилоЗаполнения = ПравилаИнтеграцииМенеджер.НовоеПравилоЗаполненияРеквизита();
	ПравилаИнтеграцииМенеджер.ДобавитьПарноеПравило(Правило, "Организация", "organization", ПравилоЗаполнения);
	ПравилаИнтеграцииМенеджер.ДобавитьПарноеПравило(Правило, "ВалютаДокумента", "currency", ПравилоЗаполнения);
	ПравилаИнтеграцииМенеджер.ДобавитьПарноеПравило(Правило, "СуммаДокумента", "sum", ПравилоЗаполнения);
	ПравилаИнтеграцииМенеджер.ДобавитьПарноеПравило(Правило, "Контрагент", "correspondent", ПравилоЗаполнения);
	ПравилаИнтеграцииМенеджер.ДобавитьПарноеПравило(Правило, "Дата", "regDate", ПравилоЗаполнения);
	ПравилаИнтеграцииМенеджер.ДобавитьПарноеПравило(Правило, "ПодразделениеОрганизации", "subdivision", ПравилоЗаполнения);
	ПравилаИнтеграцииМенеджер.ДобавитьПарноеПравило(Правило, "Комментарий", "comment", ПравилоЗаполнения);
	ПравилаИнтеграцииМенеджер.ДобавитьПарноеПравило(Правило, "Ответственный", "responsible", ПравилоЗаполнения);
	
	// Прочие правила. Выгрузка.
	
	// заголовок
	ПравилаИнтеграцииМенеджер.ДобавитьПравилоЗаполненияРеквизитаДО(
		Правило,
		Перечисления.ВариантыПравилЗаполненияРеквизитов.ВыражениеНаВстроенномЯзыке,
		"title",
		ПравилаИнтеграцииМенеджер.НовоеПравилоЗаполненияРеквизита(ШаблоныВыражений["ЗаполнениеЗаголовокаДО"]));
	
	// гриф доступа
	ПравилоЗаполнения = ПравилаИнтеграцииМенеджер.НовоеПравилоЗаполненияРеквизита(
		ШаблоныВыражений["ЗаполнениеГрифаДоступаДО"]);
	ПравилоЗаполнения.ОбновлятьДО = Ложь;
	ПравилаИнтеграцииМенеджер.ДобавитьПравилоЗаполненияРеквизитаДО(
		Правило,
		Перечисления.ВариантыПравилЗаполненияРеквизитов.ВыражениеНаВстроенномЯзыке,
		"accessLevel",
		ПравилоЗаполнения);
	
	// вопрос деятельности
	ПравилоЗаполнения = ПравилаИнтеграцииМенеджер.НовоеПравилоЗаполненияРеквизита(
		ШаблоныВыражений["ЗаполнениеВопросаДеятельностиДО"]);
	ПравилоЗаполнения.ОбновлятьДО = Ложь;
	ПравилаИнтеграцииМенеджер.ДобавитьПравилоЗаполненияРеквизитаДО(
		Правило,
		Перечисления.ВариантыПравилЗаполненияРеквизитов.ВыражениеНаВстроенномЯзыке,
		"activityMatter",
		ПравилоЗаполнения);
	
	// Прочие правила. Загрузка.
	
	ПравилоЗаполнения = ПравилаИнтеграцииМенеджер.НовоеПравилоЗаполненияРеквизита("externalNumber");
	ПравилоЗаполнения.РежимИзменения = Перечисления.РежимИзмененияПроведенногоДокументаДанными1СДокументооборота.Запрещено;
	ПравилаИнтеграцииМенеджер.ДобавитьПравилоЗаполненияРеквизитаИС(
		Правило,
		Перечисления.ВариантыПравилЗаполненияРеквизитов.РеквизитОбъекта,
		"НомерВходящегоДокумента",
		ПравилоЗаполнения);
	
	ПравилоЗаполнения = ПравилаИнтеграцииМенеджер.НовоеПравилоЗаполненияРеквизита("externalDate");
	ПравилоЗаполнения.РежимИзменения = Перечисления.РежимИзмененияПроведенногоДокументаДанными1СДокументооборота.Запрещено;
	ПравилаИнтеграцииМенеджер.ДобавитьПравилоЗаполненияРеквизитаИС(
		Правило,
		Перечисления.ВариантыПравилЗаполненияРеквизитов.РеквизитОбъекта,
		"ДатаВходящегоДокумента",
		ПравилоЗаполнения);
	
	Правило.Записать();
	
	Результат = Новый Структура;
	Результат.Вставить("Ссылка", Правило.Ссылка);
	Результат.Вставить("ТипОбъектаИС", Правило.ТипОбъектаИС);
	Результат.Вставить("ТипОбъектаДО", Правило.ТипОбъектаДО);
	Результат.Вставить("ПредставлениеОбъектаИС", Правило.ТипОбъектаИС);
	Результат.Вставить("ПредставлениеОбъектаДО", Правило.ТипОбъектаДО);
	Результат.Вставить("ИдентификаторВидаДокумента", ВидДокумента.objectID.ID);
	Результат.Вставить("ТипВидаДокумента", ВидДокумента.objectID.type);
	
	Возврат Результат;
	
КонецФункции

// Используется только для интеграции с ДО 3. Заполняет значение перечисления из XDTO пакета DM.
//
// Параметры:
//   ОбъектXDTO - Строка - значение перечисления.
//   СсылкаНаПотребитель - ЛюбаяСсылка - ссылка на исходный объект конфигурации-потребителя.
//   Настройки - см. ИнтеграцияС1СДокументооборотБазоваяФункциональностьВызовСервера.ИспользоватьИнтеграцию
//
Процедура ЗаполнитьПеречислениеXDTOИзПотребителя(ОбъектXDTO, СсылкаНаПотребитель, Настройки)
	
	Если Настройки.ИспользоватьИнтеграциюДО3 = Ложь Тогда
		Возврат;
	КонецЕсли;
	
	Если ТипЗнч(СсылкаНаПотребитель) = Тип("ПеречислениеСсылка.ЮридическоеФизическоеЛицо") Тогда
		
		Если СсылкаНаПотребитель = Перечисления.ЮридическоеФизическоеЛицо.ЮридическоеЛицо Тогда
			ОбъектXDTO = "LglPerson";
			
		ИначеЕсли СсылкаНаПотребитель = Перечисления.ЮридическоеФизическоеЛицо.ФизическоеЛицо Тогда
			ОбъектXDTO = "Individ";
			
		КонецЕсли;
		
	ИначеЕсли ТипЗнч(СсылкаНаПотребитель) = Тип("ПеречислениеСсылка.СтавкиНДС") Тогда
		
		Если СсылкаНаПотребитель = Перечисления.СтавкиНДС.БезНДС Тогда
			ОбъектXDTO = "WithoutVAT";
			
		ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС0 Тогда
			ОбъектXDTO = "VAT0";
			
		ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС10
				Или СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС10_110 Тогда
			ОбъектXDTO = "VAT10";
			
		ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС18
				Или СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС18_118 Тогда
			ОбъектXDTO = "VAT18";
			
		ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС20
				Или СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС20_120 Тогда
			ОбъектXDTO = "VAT20";
			
		КонецЕсли;
		
	КонецЕсли;
	
КонецПроцедуры

Процедура ЗаполнитьСсылочныйОбъектXDTOИзПотребителя(Прокси, ОбъектXDTO, СсылкаНаПотребитель, Настройки)
	
	УстановитьПривилегированныйРежим(Истина);
	
	Если ТипЗнч(СсылкаНаПотребитель) = Тип("ПеречислениеСсылка.ЮридическоеФизическоеЛицо") Тогда
		
		Если Настройки.ИспользоватьИнтеграциюДО2 Тогда
			ОбъектXDTO = ИнтеграцияС1СДокументооборотБазоваяФункциональность.СоздатьОбъект(
				Прокси,
				"DMLegalPrivatePerson",
				СсылкаНаПотребитель,
				Ложь);
			ОбъектXDTO.objectID.type = "DMLegalPrivatePerson";
			Если СсылкаНаПотребитель = Перечисления.ЮридическоеФизическоеЛицо.ЮридическоеЛицо Тогда
				ОбъектXDTO.objectID.ID = "ЮрЛицо";
			ИначеЕсли СсылкаНаПотребитель = Перечисления.ЮридическоеФизическоеЛицо.ФизическоеЛицо Тогда
				ОбъектXDTO.objectID.ID = "ФизЛицо";
			КонецЕсли;
		ИначеЕсли Настройки.ИспользоватьИнтеграциюДО3 Тогда
			ЗаполнитьПеречислениеXDTOИзПотребителя(ОбъектXDTO, СсылкаНаПотребитель, Настройки);
		КонецЕсли;
		
	ИначеЕсли ТипЗнч(СсылкаНаПотребитель) = Тип("ПеречислениеСсылка.СтавкиНДС") Тогда
		
		Если Настройки.ИспользоватьИнтеграциюДО2 Тогда
			ОбъектXDTO = ИнтеграцияС1СДокументооборотБазоваяФункциональность.СоздатьОбъект(
				Прокси,
				"DMVATRate",
				СсылкаНаПотребитель,
				Ложь);
			ОбъектXDTO.objectID.type = "DMVATRate";
			Если СсылкаНаПотребитель = Перечисления.СтавкиНДС.БезНДС Тогда
				ОбъектXDTO.objectID.ID = "БезНДС";
			ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС0 Тогда
				ОбъектXDTO.objectID.ID = "НДС0";
			ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС5
					Или СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС5_105 Тогда
				ОбъектXDTO.objectID.ID = "НДС5";
			ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС7
					Или СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС7_107 Тогда
				ОбъектXDTO.objectID.ID = "НДС7";
			ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС10
					Или СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС10_110 Тогда
				ОбъектXDTO.objectID.ID = "НДС10";
			ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС18
					Или СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС18_118 Тогда
				ОбъектXDTO.objectID.ID = "НДС18";
			ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС20
					Или СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС20_120 Тогда
				ОбъектXDTO.objectID.ID = "НДС20";
			ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС22
					Или СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС22_122 Тогда
				ОбъектXDTO.objectID.ID = "НДС22";
			КонецЕсли;
		ИначеЕсли Настройки.ИспользоватьИнтеграциюДО3 Тогда
			Если ИнтеграцияС1СДокументооборотБазоваяФункциональностьПовтИсп.ДоступенФункционалВерсииСервиса("3.0.16.33") Тогда
				ОбъектXDTO = ИнтеграцияС1СДокументооборотБазоваяФункциональность.СоздатьОбъект(
					Прокси,
					"DMVATRateUniversal",
					СсылкаНаПотребитель,
					Ложь);
				Если СсылкаНаПотребитель = Перечисления.СтавкиНДС.БезНДС Тогда
					ОбъектXDTO.predefinedName = "WithoutVAT";
					ОбъектXDTO.rate = 0;
				ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС0 Тогда
					ОбъектXDTO.predefinedName = "VAT0";
					ОбъектXDTO.rate = 0;
				ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС5 Тогда
					ОбъектXDTO.rate = 5;
				ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС5_105 Тогда
					ОбъектXDTO.rate = 5;
					ОбъектXDTO.calculatedRate = Истина;
				ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС7 Тогда
					ОбъектXDTO.rate = 7;
				ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС7_107 Тогда
					ОбъектXDTO.rate = 7;
					ОбъектXDTO.calculatedRate = Истина;
				ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС10 Тогда
					ОбъектXDTO.predefinedName = "VAT10";
					ОбъектXDTO.rate = 10;
				ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС10_110 Тогда
					ОбъектXDTO.predefinedName = "VAT10";
					ОбъектXDTO.rate = 10;
					ОбъектXDTO.calculatedRate = Истина;
				ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС18 Тогда
					ОбъектXDTO.predefinedName = "VAT18";
					ОбъектXDTO.rate = 18;
				ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС18_118 Тогда
					ОбъектXDTO.predefinedName = "VAT18";
					ОбъектXDTO.rate = 18;
					ОбъектXDTO.calculatedRate = Истина;
				ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС20 Тогда
					ОбъектXDTO.predefinedName = "VAT20";
					ОбъектXDTO.rate = 20;
				ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС20_120 Тогда
					ОбъектXDTO.predefinedName = "VAT20";
					ОбъектXDTO.rate = 20;
					ОбъектXDTO.calculatedRate = Истина;
				ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС22 Тогда
					ОбъектXDTO.rate = 22;
				ИначеЕсли СсылкаНаПотребитель = Перечисления.СтавкиНДС.НДС22_122 Тогда
					ОбъектXDTO.rate = 22;
					ОбъектXDTO.calculatedRate = Истина;
				КонецЕсли;
			Иначе
				ЗаполнитьПеречислениеXDTOИзПотребителя(ОбъектXDTO, СсылкаНаПотребитель, Настройки);
			КонецЕсли;
		КонецЕсли;
		
	ИначеЕсли (ИнтеграцияС1СДокументооборотБазоваяФункциональность.ПроверитьТип(Прокси, ОбъектXDTO, "DMCorrespondent")
				И ТипЗнч(СсылкаНаПотребитель) = Тип("СправочникСсылка.Контрагенты")
				И Не ОбщегоНазначения.ЗначениеРеквизитаОбъекта(СсылкаНаПотребитель, "ЭтоГруппа"))
			Или (ИнтеграцияС1СДокументооборотБазоваяФункциональность.ПроверитьТип(Прокси, ОбъектXDTO, "DMOrganization")
				И ТипЗнч(СсылкаНаПотребитель) = Тип("СправочникСсылка.Организации")) Тогда
		
		Реквизиты = ОбщегоНазначения.ЗначенияРеквизитовОбъекта(СсылкаНаПотребитель,
			"ИНН, КПП, ЮридическоеФизическоеЛицо, ИндивидуальныйПредприниматель, КодПоОКПО, НаименованиеПолное");
		
		Если ЗначениеЗаполнено(Реквизиты.ИНН) Тогда
			ОбъектXDTO.inn = Реквизиты.ИНН;
		КонецЕсли;
		Если ЗначениеЗаполнено(Реквизиты.КПП) Тогда
			ОбъектXDTO.kpp = Реквизиты.КПП;
		КонецЕсли;
		Если ЗначениеЗаполнено(Реквизиты.КодПоОКПО) Тогда
			ОбъектXDTO.okpo = Реквизиты.КодПоОКПО;
		КонецЕсли;
		Если ЗначениеЗаполнено(Реквизиты.НаименованиеПолное) Тогда
			ОбъектXDTO.fullName = Реквизиты.НаименованиеПолное;
		КонецЕсли;
		
		ЭтоИП = Ложь;
		Если ТипЗнч(Реквизиты.ИндивидуальныйПредприниматель) = Тип("СправочникСсылка.ФизическиеЛица") Тогда
			ЭтоИП = ЗначениеЗаполнено(Реквизиты.ИндивидуальныйПредприниматель);
		Иначе
			ЭтоИП = (Реквизиты.ИндивидуальныйПредприниматель = Истина);
		КонецЕсли;
		
		Если ЭтоИП Тогда
			Если Настройки.ИспользоватьИнтеграциюДО2 Тогда
				ОбъектXDTO.legalPrivatePerson = ИнтеграцияС1СДокументооборотБазоваяФункциональность.СоздатьОбъект(
					Прокси,
					"DMLegalPrivatePerson");
				ОбъектXDTO.legalPrivatePerson.objectID.type = "DMLegalPrivatePerson";
				ОбъектXDTO.legalPrivatePerson.objectID.ID = "ИндивидуальныйПредприниматель";
				ОбъектXDTO.legalPrivatePerson.name = НСтр("ru = 'Индивидуальный предприниматель'");
			ИначеЕсли Настройки.ИспользоватьИнтеграциюДО3 Тогда
				ОбъектXDTO.legalPrivatePerson = "IndividualEntrepreneur";
			КонецЕсли;
		Иначе
			ИнтеграцияС1СДокументооборотБазоваяФункциональность.ЗаполнитьРеквизитыИзПотребителя(
				Прокси,
				ОбъектXDTO.legalPrivatePerson,
				Реквизиты.ЮридическоеФизическоеЛицо);
		КонецЕсли;
		
	ИначеЕсли ИнтеграцияС1СДокументооборотБазоваяФункциональность.ПроверитьТип(Прокси, ОбъектXDTO, "DMContactPerson")
			И ТипЗнч(СсылкаНаПотребитель) = Тип("СправочникСсылка.КонтактныеЛица") Тогда
		
		Реквизиты = ОбщегоНазначения.ЗначенияРеквизитовОбъекта(СсылкаНаПотребитель,
			"ОбъектВладелец, Должность, Описание");
		
		ОбъектXDTO.position = Реквизиты.Должность;
		ОбъектXDTO.comment = Реквизиты.Описание;
		
		Если ЗначениеЗаполнено(Реквизиты.ОбъектВладелец)
				И ТипЗнч(Реквизиты.ОбъектВладелец) = Тип("СправочникСсылка.Контрагенты") Тогда
			ОбъектXDTO.correspondent = ИнтеграцияС1СДокументооборотБазоваяФункциональность.СоздатьОбъект(
				Прокси,
				"DMCorrespondent",
				Реквизиты.ОбъектВладелец);
		КонецЕсли;
		
	ИначеЕсли ИнтеграцияС1СДокументооборотБазоваяФункциональность.ПроверитьТип(Прокси, ОбъектXDTO, "DMPrivatePerson")
			И ТипЗнч(СсылкаНаПотребитель) = Тип("СправочникСсылка.ФизическиеЛица")
			И Не ОбщегоНазначения.ЗначениеРеквизитаОбъекта(СсылкаНаПотребитель, "ЭтоГруппа") Тогда
		
		Реквизиты = ОбщегоНазначения.ЗначенияРеквизитовОбъекта(СсылкаНаПотребитель,
			"ФИО, ДатаРождения, ИНН, СтраховойНомерПФР");
		
		Если ЗначениеЗаполнено(Реквизиты.ФИО) Тогда
			ОбъектXDTO.name = Реквизиты.ФИО;
		КонецЕсли;
		Если ЗначениеЗаполнено(Реквизиты.ДатаРождения) Тогда
			ОбъектXDTO.birthDay = Реквизиты.ДатаРождения;
		КонецЕсли;
		Если ЗначениеЗаполнено(Реквизиты.ИНН)
				И ИнтеграцияС1СДокументооборотБазоваяФункциональность.СвойствоСуществует(ОбъектXDTO, "inn") Тогда
			ОбъектXDTO.inn = Реквизиты.ИНН;
		КонецЕсли;
		Если ЗначениеЗаполнено(Реквизиты.СтраховойНомерПФР)
				И ИнтеграцияС1СДокументооборотБазоваяФункциональность.СвойствоСуществует(ОбъектXDTO, "retirementInsuranceNumber") Тогда
			ОбъектXDTO.retirementInsuranceNumber = Реквизиты.СтраховойНомерПФР;
		КонецЕсли;
		
	ИначеЕсли ИнтеграцияС1СДокументооборотБазоваяФункциональность.ПроверитьТип(Прокси, ОбъектXDTO, "DMBankAccount")
			И ТипЗнч(СсылкаНаПотребитель) = Тип("СправочникСсылка.БанковскиеСчета") Тогда
		
		Реквизиты = ОбщегоНазначения.ЗначенияРеквизитовОбъекта(СсылкаНаПотребитель, "Владелец, Банк, НомерСчета");
		
		ОбъектXDTO.number = Реквизиты.НомерСчета;
		
		Если ЗначениеЗаполнено(Реквизиты.Владелец) Тогда
			Если ТипЗнч(Реквизиты.Владелец) = Тип("СправочникСсылка.Контрагенты") Тогда
				ТипОбъектаXDTO = "DMCorrespondent";
			ИначеЕсли ТипЗнч(Реквизиты.Владелец) = Тип("СправочникСсылка.ФизическиеЛица") Тогда
				ТипОбъектаXDTO = "DMPrivatePerson";
			ИначеЕсли ТипЗнч(Реквизиты.Владелец) = Тип("СправочникСсылка.Организации") Тогда
				ТипОбъектаXDTO = "DMOrganization";
			КонецЕсли;
