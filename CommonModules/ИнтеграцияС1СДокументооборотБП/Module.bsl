// Ищет подходящее значение перечисления ЮрФизЛицо из реквизита legalPrivatePerson объекта XDTO типа DMCorrespondent или
// DMOrganization.
//
// Параметры:
//   ОбъектXDTO - ОбъектXDTO - исходный объект, соответствие которому не найдено в текущей ИБ.
//   ИндивидуальныйПредприниматель - Булево, СправочникСсылка.ФизическиеЛица - неявно возвращаемое значение,
//     заполняется если лицо является индивидуальным предпринимателем.
//
// Возвращаемое значение:
//   ПеречислениеСсылка.ЮрФизЛицо
//
Функция ЮрФизЛицоИзОбъектаXDTO(ОбъектXDTO, ИндивидуальныйПредприниматель) Экспорт
	
	ЮрФизЛицо = Перечисления.ЮридическоеФизическоеЛицо.ЮридическоеЛицо;
	Если ТипЗнч(ИндивидуальныйПредприниматель) = Тип("СправочникСсылка.ФизическиеЛица") Тогда
		ИндивидуальныйПредприниматель = Справочники.ФизическиеЛица.ПустаяСсылка();
	Иначе
		ИндивидуальныйПредприниматель = Ложь;
	КонецЕсли;
	
	Если ОбъектXDTO = Неопределено
			Или Не ИнтеграцияС1СДокументооборотБазоваяФункциональность.СвойствоУстановлено(ОбъектXDTO, "legalPrivatePerson") Тогда
		Возврат ЮрФизЛицо;
	КонецЕсли;
	
	ЭтоИП = Ложь;
	
	Настройки = ИнтеграцияС1СДокументооборотБазоваяФункциональностьВызовСервера.ИспользоватьИнтеграцию();
	Если Настройки.ИспользоватьИнтеграциюДО2 Тогда
		
		Если ОбъектXDTO.legalPrivatePerson.objectID.ID = "ФизЛицо" Тогда //@NON-NLS-1
			ЮрФизЛицо = Перечисления.ЮридическоеФизическоеЛицо.ФизическоеЛицо;
			
		ИначеЕсли ОбъектXDTO.legalPrivatePerson.objectID.ID = "ЮрЛицоНеРезидент" Тогда //@NON-NLS-1
			ЮрФизЛицо = Перечисления.ЮридическоеФизическоеЛицо.ЮридическоеЛицо;
			
		ИначеЕсли ОбъектXDTO.legalPrivatePerson.objectID.ID = "ИндивидуальныйПредприниматель" Тогда //@NON-NLS-1
			ЮрФизЛицо = Перечисления.ЮридическоеФизическоеЛицо.ФизическоеЛицо;
			ЭтоИП = Истина;
			
		КонецЕсли;
		
	ИначеЕсли Настройки.ИспользоватьИнтеграциюДО3 Тогда
		
		Если ОбъектXDTO.legalPrivatePerson = "Individ" Тогда
			ЮрФизЛицо = Перечисления.ЮридическоеФизическоеЛицо.ФизическоеЛицо;
			
		ИначеЕсли ОбъектXDTO.legalPrivatePerson = "LglPersonNonResident" Тогда
			ЮрФизЛицо = Перечисления.ЮридическоеФизическоеЛицо.ЮридическоеЛицо;
			
		ИначеЕсли ОбъектXDTO.legalPrivatePerson = "IndividualEntrepreneur" Тогда
			ЮрФизЛицо = Перечисления.ЮридическоеФизическоеЛицо.ФизическоеЛицо;
			ЭтоИП = Истина;
			
		КонецЕсли;
		
	КонецЕсли;
	
	Если ЭтоИП Тогда
		Если ТипЗнч(ИндивидуальныйПредприниматель) = Тип("СправочникСсылка.ФизическиеЛица") Тогда
			ФизическоеЛицоСсылка = Справочники.ФизическиеЛица.НайтиПоНаименованию(ОбъектXDTO.name, Истина);
			Если Не ЗначениеЗаполнено(ФизическоеЛицоСсылка) Тогда
				ФизическоеЛицо = Справочники.ФизическиеЛица.СоздатьЭлемент();
				ФизическоеЛицо.Заполнить(Неопределено);
				ФизическоеЛицо.Наименование = ОбъектXDTO.name;
				ФизическоеЛицо.ФИО = ОбъектXDTO.name;
				ФизическоеЛицо.Записать();
				ФизическоеЛицоСсылка = ФизическоеЛицо.Ссылка;
			КонецЕсли;
			ИндивидуальныйПредприниматель = ФизическоеЛицоСсылка;
		Иначе
			ИндивидуальныйПредприниматель = Истина;
		КонецЕсли;
	КонецЕсли;
	
	Возврат ЮрФизЛицо;
	
КонецФункции

