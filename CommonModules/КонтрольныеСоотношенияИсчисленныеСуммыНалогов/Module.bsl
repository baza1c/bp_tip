Функция СрокУплатыНалога(КонтекстПроверки, ТекущиеДанные) // для КС 77
	
	СрокУплаты = Дата(1, 1, 1);
	
	Год = Число(" " + ТекущиеДанные.ГодОтч);
	
	МассивПериодов = КонтекстПроверки.ДанныеКС.ТаблицаПериодов[ТекущиеДанные.КБК];
	Если МассивПериодов = Неопределено Тогда
		Возврат СрокУплаты;
	КонецЕсли;
	
	ИндексПериода = МассивПериодов.Найти(ТекущиеДанные.КодПер);
	Если ИндексПериода = Неопределено Тогда
		Возврат СрокУплаты;
	КонецЕсли;
	НомерПериода = ИндексПериода + 1;
	
	ГодУплаты   = Год;
	ДеньУплаты  = 28; // общий порядок
	
	Если СтрНайти(ТекущиеДанные.КНД, "1151100") > 0 Тогда // "6-НДФЛ" ("3-НДФЛ")
		
		Если НомерПериода > 24 Тогда // "3-НДФЛ"
			НомерПериода = НомерПериода - 24;
			МесяцУплаты = НомерПериода * 3 + 1;
			ТекущиеДанные.КНД = "1151020";
		Иначе
			МесяцУплаты = Цел(НомерПериода / 2) + 1;
			Если МесяцУплаты = 13 Тогда
				МесяцУплаты = 12;
				ДеньУплаты  = 31;
			ИначеЕсли НомерПериода % 2 = 0 Тогда // четный номер срока уплаты
				ДеньУплаты = 5;
			КонецЕсли;
		КонецЕсли;
		
	ИначеЕсли СтрНайти(ТекущиеДанные.КНД, "1151020") > 0 Тогда // "3-НДФЛ"
		
		МесяцУплаты = НомерПериода * 3 + 1;
		
	ИначеЕсли СтрНайти(ТекущиеДанные.КНД, "1152017") > 0 Тогда // "Декларация по УСН"
		
		МесяцУплаты = НомерПериода * 3 + 1;
		
	ИначеЕсли СтрНайти(ТекущиеДанные.КНД, "1151059") > 0 Тогда // "Декларация по ЕСХН"
		
		МесяцУплаты = 7;
		
	Иначе
		
		МесяцУплаты = НомерПериода + 1;
		
		Если МесяцУплаты = 13 Тогда
			ГодУплаты = ГодУплаты + 1;
			МесяцУплаты = 1;
		КонецЕсли;
		
	КонецЕсли;
	
	Возврат Дата(ГодУплаты, МесяцУплаты, ДеньУплаты);
	
КонецФункции

