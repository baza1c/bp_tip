Процедура ПроверкаКС_77_код_данные(КонтекстПроверки, ПараметрыОтбора)
	
	ПараметрыОтбора = Новый Структура;
	ПараметрыОтбора.Вставить("СтруктураДанных", Новый Структура);
	
	ТаблицаОтчеты = Новый ТаблицаЗначений;
	ТаблицаОтчеты.Колонки.Добавить("НС",      Новый ОписаниеТипов("Строка"));
	ТаблицаОтчеты.Колонки.Добавить("ДатаДок", Новый ОписаниеТипов("Дата", , , Новый КвалификаторыДаты(ЧастиДаты.Дата)));
	ТаблицаОтчеты.Колонки.Добавить("Организация");
	
	ТаблицаОтчеты.Колонки.Добавить("КНД",     Новый ОписаниеТипов("Строка"));
	ТаблицаОтчеты.Колонки.Добавить("КБК",     Новый ОписаниеТипов("Строка", , Новый КвалификаторыСтроки(20)));
	ТаблицаОтчеты.Колонки.Добавить("ОКТМО",   Новый ОписаниеТипов("Строка", , Новый КвалификаторыСтроки(11)));
	ТаблицаОтчеты.Колонки.Добавить("Период",  Новый ОписаниеТипов("Строка", , Новый КвалификаторыСтроки(2)));
	ТаблицаОтчеты.Колонки.Добавить("МесПер",  Новый ОписаниеТипов("Строка", , Новый КвалификаторыСтроки(2)));
	ТаблицаОтчеты.Колонки.Добавить("КодПер",  Новый ОписаниеТипов("Строка"));
	ТаблицаОтчеты.Колонки.Добавить("ГодОтч",  Новый ОписаниеТипов("Строка"));
	ТаблицаОтчеты.Колонки.Добавить("СрокУпл", Новый ОписаниеТипов("Дата", , , Новый КвалификаторыДаты(ЧастиДаты.Дата)));
	ТаблицаОтчеты.Колонки.Добавить("Сумма",   Новый ОписаниеТипов("Число"));
	ТаблицаОтчеты.Колонки.Добавить("Данные");
	
	ПараметрыОтбора.СтруктураДанных.Вставить("ТаблицаОтчеты", ТаблицаОтчеты);
	
	Запрос = Новый Запрос("ВЫБРАТЬ РАЗРЕШЕННЫЕ
	|	Организации.Ссылка КАК Организация,
	|	Организации.ИНН КАК ИНН,
	|	Организации.КПП КАК КПП
	|ИЗ
	|	Справочник.Организации КАК Организации
	|ГДЕ
	|	Организации.ИНН ПОДОБНО ""%"" + &ИНН + ""%""
	|	И Организации.ПометкаУдаления = ЛОЖЬ");
	
	Запрос.УстановитьПараметр("ИНН", КонтекстПроверки.ДанныеКС.ИНН);
	
	Организации = Запрос.Выполнить().Выгрузить();
	
	КомплектКБК         = КонтекстПроверки.НастройкиКС.КБК;
	СоответствиеКБК_КНД = КонтекстПроверки.ДанныеКС.КБК_КНД;
	
	Для каждого СтраницаРаздела Из КонтекстПроверки.ДанныеКС.Р1 Цикл
		
		КБК = СокрЛП(ОбщегоНазначенияКлиентСервер.СвойствоСтруктуры(СтраницаРаздела, "КБК", ""));
		Если КомплектКБК.Найти(КБК) = Неопределено Тогда
			Продолжить;
		КонецЕсли;
		
		КПП = СокрЛП(ОбщегоНазначенияКлиентСервер.СвойствоСтруктуры(СтраницаРаздела, "КППДекл", ""));
		Организация = Неопределено;
		Для каждого Стр Из Организации Цикл
			Если СокрЛП(Стр.КПП) = КПП Тогда
				Организация = Стр.Организация;
				Прервать;
			КонецЕсли;
		КонецЦикла;
		Если Организация = Неопределено Тогда
			Продолжить;
		КонецЕсли;
		
		НовСтрока = ТаблицаОтчеты.Добавить();
		НовСтрока.НС = СокрЛП(ОбщегоНазначенияКлиентСервер.СвойствоСтруктуры(СтраницаРаздела, "НомерСтраницы", ""));
		
		НовСтрока.ДатаДок = КонтекстПроверки.НастройкиКС.ДатаПодписи;
		НовСтрока.Организация = Организация;
		
		НовСтрока.КБК     = КБК;
		НовСтрока.КНД     = СоответствиеКБК_КНД[КБК];
		НовСтрока.ОКТМО   = СокрЛП(ОбщегоНазначенияКлиентСервер.СвойствоСтруктуры(СтраницаРаздела, "ОКТМО", ""));
		НовСтрока.Период  = СокрЛП(ОбщегоНазначенияКлиентСервер.СвойствоСтруктуры(СтраницаРаздела, "Период", ""));
		НовСтрока.КодПер  = СокрЛП(ОбщегоНазначенияКлиентСервер.СвойствоСтруктуры(СтраницаРаздела, "КодПериода", ""));
		НовСтрока.ГодОтч  = СокрЛП(ОбщегоНазначенияКлиентСервер.СвойствоСтруктуры(СтраницаРаздела, "Год", ""));
		НовСтрока.Сумма   = СокрЛП(ОбщегоНазначенияКлиентСервер.СвойствоСтруктуры(СтраницаРаздела, "СумНалогАванс", ""));
		
		НовСтрока.СрокУпл = СрокУплатыНалога(КонтекстПроверки, НовСтрока);
		
	КонецЦикла;
	
	РегламентированнаяОтчетностьДанныеОтчетов.ПолучитьДанныеОтчетов(ПараметрыОтбора);
	
КонецПроцедуры

