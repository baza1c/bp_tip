Процедура ПроверкаКС_01(КонтекстПроверки)
	
	Заголовки = Новый Структура;
	Заголовки.Вставить("Раздел"				, НСтр("ru='Сведения о суммах налогов'"));
	Заголовки.Вставить("НомерКС"			, НСтр("ru='01'"));
