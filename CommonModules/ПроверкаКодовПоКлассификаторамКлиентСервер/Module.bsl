
#Область ПрограммныйИнтерфейс

// Конструктор для коллекции, описывающий реализацию конкретной формы.
// 
// Возвращаемое значение:
//  Структура - см. тело функции
//
Функция НовыйОписаниеФормы() Экспорт
	
	ОписаниеФормы = Новый Структура;
	
	ОписаниеФормы.Вставить("РеквизитКодПоОКТМО", "РегистрацияВНалоговомОрганеКодПоОКТМО");
	ОписаниеФормы.Вставить("ДекорацияРезультатПроверкиКодаПоОКТМО", "НадписьПояснениеПроверкиКодаПоОКТМО");
	ОписаниеФормы.Вставить("ГруппаКонтактнаяИнформация", "ГруппаКонтактнаяИнформация");
	
	Возврат ОписаниеФормы;
	
КонецФункции

// Конструктор для новой проверки кодов.
// 
// Возвращаемое значение:
//  Структура - см. тело функции
//
Функция НовыйПроверка() Экспорт
	
	Проверка = Новый Структура;
	
	Проверка.Вставить("ПараметрыПроверки", НовыйПараметрыПроверки());
	Проверка.Вставить("Адрес");
	Проверка.Вставить("РезультатПроверки", НовыйРезультатПроверки());
	
	Возврат Проверка;
	
КонецФункции

// Конструктор параметров для новой проверки кодов.
// 
// Возвращаемое значение:
//  Структура - см. тело функции
//
Функция НовыйПараметрыПроверки() Экспорт
	
	ПараметрыПроверкиОКТМО = Новый Структура;
	
	ПараметрыПроверкиОКТМО.Вставить("Объект");
	ПараметрыПроверкиОКТМО.Вставить("ВидКонтактнойИнформации",
		ПредопределенноеЗначение("Справочник.ВидыКонтактнойИнформации.ЮрАдресОрганизации"));
	ПараметрыПроверкиОКТМО.Вставить("ОКТМО", "");
	
	ПараметрыПроверки = Новый Структура;
	ПараметрыПроверки.Вставить("КодПоОКТМО", ПараметрыПроверкиОКТМО);
	
	ПараметрыПроверкиКБК = Новый Структура;
	ПараметрыПроверкиКБК.Вставить("КБК", "");
	ПараметрыПроверкиКБК.Вставить("Дата", Дата(1,1,1));
	
	ПараметрыПроверки.Вставить("КодБК", ПараметрыПроверкиКБК);
	
	Возврат ПараметрыПроверки;
	
КонецФункции

// Конструктор результатов проверки кодов.
// 
// Возвращаемое значение:
//  Структура - см. тело функции
//
Функция НовыйРезультатПроверки() Экспорт
	
	РезультатПроверкиОКТМО = Новый Структура;
	РезультатПроверкиОКТМО.Вставить("Успешно");
	РезультатПроверкиОКТМО.Вставить("ОжидаемыйКодПоОКТМО", "");
	РезультатПроверкиОКТМО.Вставить("ТекстОшибки", "");
	
	РезультатПроверки = Новый Структура;
	РезультатПроверки.Вставить("КодПоОКТМО", РезультатПроверкиОКТМО);
	
	РезультатПроверкиКБК = Новый Структура;
	РезультатПроверкиКБК.Вставить("Успешно");
	РезультатПроверкиКБК.Вставить("ТекстОшибки");
	
	РезультатПроверки.Вставить("КодБК", РезультатПроверкиКБК);
	
	Возврат РезультатПроверки;
	
КонецФункции

#КонецОбласти
