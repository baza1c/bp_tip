#Область ПроводкиАмортизации

Функция РеквизитыАмортизации(Период, Организация, Содержание)
	
	Реквизиты = Новый Структура;
	Реквизиты.Вставить("Период",        Период);
	Реквизиты.Вставить("Организация",   Организация);
	Реквизиты.Вставить("НалоговыйУчет", УчетнаяПолитика.ПлательщикНалогаНаПрибыль(Организация, Период));
	Реквизиты.Вставить("ПБУ18",         УчетнаяПолитика.ПоддержкаПБУ18(Организация, Период));
	Реквизиты.Вставить("Содержание",    Содержание);
	
	Если ПустаяСтрока(Реквизиты.Содержание) Тогда
		Реквизиты.Содержание = НСтр("ru = 'Амортизация'", ОбщегоНазначения.КодОсновногоЯзыка());
	КонецЕсли;
	
	Возврат Реквизиты;
	
КонецФункции

Функция КешСвойстваСчетов()
	
	Кеш = Новый Структура;
	
	Кеш.Вставить("ВидыСубконто",                  Новый Соответствие);
	Кеш.Вставить("СвойстваСчетаАмортизации",      Новый Соответствие);
	Кеш.Вставить("КредитОбесценения",             Новый Соответствие);
	Кеш.Вставить("СвойстваСчетовДт",              Новый Соответствие);
	Кеш.Вставить("СчетРасходыНаНИОКР",            ПланыСчетов.Хозрасчетный.РасходыНаНИОКР);
		
	Возврат Кеш;
	
КонецФункции

Процедура ДобавитьПроводкуАмортизация(Проводки, СтрокаТаблицы, Реквизиты, ВидСубконто, КешСвойстваСчетов)
	
	Если СтрокаТаблицы.СуммаБУ = 0
		И СтрокаТаблицы.СуммаНУ = 0
		И СтрокаТаблицы.СуммаПР = 0
		И СтрокаТаблицы.СуммаВР = 0 Тогда
		Возврат;
	КонецЕсли;

	Проводка = Проводки.Добавить();

	Проводка.Период      = Реквизиты.Период;
	Проводка.Организация = Реквизиты.Организация;

	Если ЗначениеЗаполнено(СтрокаТаблицы.СодержаниеОперации) Тогда
		Проводка.Содержание = СтрокаТаблицы.СодержаниеОперации;
	ИначеЕсли СтрокаТаблицы.СчетАмортизации = КешСвойстваСчетов.СчетРасходыНаНИОКР Тогда
		Проводка.Содержание = НСтр("ru = 'Списание стоимости НИОКР'");
	Иначе
		Проводка.Содержание = Реквизиты.Содержание;
	КонецЕсли;

	Проводка.СчетДт = СтрокаТаблицы.СчетЗатрат;
	СвойстваСчетаДт = БухгалтерскийУчет.УстановитьСубконтоПоКешуСвойствСчета(
		Проводка.СчетДт,
		Проводка.СубконтоДт,
		КешСвойстваСчетов.СвойстваСчетовДт,
		СтрокаТаблицы.Субконто1,
		СтрокаТаблицы.Субконто2,
		СтрокаТаблицы.Субконто3);
	
	Если СвойстваСчетаДт.УчетПоПодразделениям Тогда
		Проводка.ПодразделениеДт = СтрокаТаблицы.ПодразделениеЗатрат;
	КонецЕсли;
	
	Проводка.СчетКт = СтрокаТаблицы.СчетАмортизации;
	
	СвойстваСчетаКт = КешСвойстваСчетов.СвойстваСчетаАмортизации[Проводка.СчетКт];
	Если СвойстваСчетаКт = Неопределено Тогда
		СвойстваСчетаКт = БухгалтерскийУчетВызовСервераПовтИсп.ПолучитьСвойстваСчета(Проводка.СчетКт);
		КешСвойстваСчетов.СвойстваСчетаАмортизации.Вставить(Проводка.СчетКт, СвойстваСчетаКт);
	КонецЕсли;
	
	Если ЗначениеЗаполнено(ВидСубконто) Тогда
		Проводка.СубконтоКт.Вставить(ВидСубконто, СтрокаТаблицы.ОбъектУчета);
	КонецЕсли;
	
	Если СвойстваСчетаКт.УчетПоПодразделениям Тогда
		Проводка.ПодразделениеКт = СтрокаТаблицы.Подразделение;
	КонецЕсли;

	Проводка.Сумма = СтрокаТаблицы.СуммаБУ;

	Если Реквизиты.НалоговыйУчет Тогда

		Если СвойстваСчетаДт.НалоговыйУчет Тогда
			Проводка.СуммаНУДт = СтрокаТаблицы.СуммаНУ;
			Если Реквизиты.ПБУ18 Тогда
				Проводка.СуммаПРДт = СтрокаТаблицы.СуммаПР;
				Проводка.СуммаВРДт = СтрокаТаблицы.СуммаВР;
			КонецЕсли;
		КонецЕсли;

		Если СвойстваСчетаКт.НалоговыйУчет Тогда
			Проводка.СуммаНУКт = СтрокаТаблицы.СуммаНУ;
			Если Реквизиты.ПБУ18 Тогда
				Проводка.СуммаПРКт = СтрокаТаблицы.СуммаПР;
				Проводка.СуммаВРКт = СтрокаТаблицы.СуммаВР;
			КонецЕсли;
		КонецЕсли;

		Если СвойстваСчетаДт.НалоговыйУчет И НалоговыйУчет.ЗапрещеноУчитыватьРасход(Проводка) Тогда
			
			Если Реквизиты.ПБУ18 Тогда
				Проводка.СуммаПРДт = Проводка.СуммаПРДт + Проводка.СуммаНУДт;
			КонецЕсли;
			
			Проводка.СуммаНУДт = 0;
			
		КонецЕсли;
		
	КонецЕсли;
	
КонецПроцедуры

Процедура ДобавитьПроводкуКорректировкаОбесценения(Проводки, СтрокаТаблицы, Реквизиты, ВидСубконто, ТипОбъекта, КешСвойстваСчетов)
		
	Если СтрокаТаблицы.КорректировкаОбесценения = 0 Тогда
		Возврат;
	КонецЕсли;
	
	КредитОбесценения = КешСвойстваСчетов.КредитОбесценения[ТипОбъекта];
	Если КредитОбесценения = Неопределено Тогда
		КредитОбесценения = КредитОбесценения(ТипОбъекта);
		КешСвойстваСчетов.КредитОбесценения.Вставить(ТипОбъекта, КредитОбесценения);
	КонецЕсли;
	
	Если Не ЗначениеЗаполнено(КредитОбесценения.Счет) Тогда
		Возврат;
	КонецЕсли;

	КредитОбесценения = КешСвойстваСчетов.КредитОбесценения[ТипОбъекта];
	Если КредитОбесценения = Неопределено Тогда
		КредитОбесценения = КредитОбесценения(ТипОбъекта);
		КешСвойстваСчетов.КредитОбесценения.Вставить(ТипОбъекта, КредитОбесценения);
	КонецЕсли;
	
	Если Не ЗначениеЗаполнено(КредитОбесценения) Тогда
		Возврат;
	КонецЕсли;

	Проводка = Проводки.Добавить();

	Проводка.Период      = Реквизиты.Период;
	Проводка.Организация = Реквизиты.Организация;
	
	Если ЗначениеЗаполнено(СтрокаТаблицы.СодержаниеОперации) Тогда
		Проводка.Содержание = СтрокаТаблицы.СодержаниеОперации;
	Иначе
		Проводка.Содержание = НСтр("ru = 'Корректировка обесценения'");
	КонецЕсли;

	Проводка.СчетДт = СтрокаТаблицы.СчетЗатрат;
	СвойстваСчетаДт = БухгалтерскийУчет.УстановитьСубконтоПоКешуСвойствСчета(
		Проводка.СчетДт,
		Проводка.СубконтоДт,
		КешСвойстваСчетов.СвойстваСчетовДт,
		СтрокаТаблицы.Субконто1,
		СтрокаТаблицы.Субконто2,
		СтрокаТаблицы.Субконто3);
	
	Если СвойстваСчетаДт.УчетПоПодразделениям Тогда
		Проводка.ПодразделениеДт = СтрокаТаблицы.ПодразделениеЗатрат;
	КонецЕсли;
	
	Проводка.СчетКт = КредитОбесценения.Счет;
	СвойстваСчетаОбесценения = КредитОбесценения.СвойстваСчета;
	Если ЗначениеЗаполнено(ВидСубконто) Тогда
		Проводка.СубконтоКт.Вставить(ВидСубконто, СтрокаТаблицы.ОбъектУчета);
	КонецЕсли;
	
	Если СвойстваСчетаОбесценения.УчетПоПодразделениям Тогда
		Проводка.ПодразделениеКт = СтрокаТаблицы.Подразделение;
	КонецЕсли;

	Проводка.Сумма = - СтрокаТаблицы.КорректировкаОбесценения;

	Если Реквизиты.НалоговыйУчет Тогда

		Если СвойстваСчетаДт.НалоговыйУчет Тогда
			Проводка.СуммаНУДт = 0;
			Если Реквизиты.ПБУ18 Тогда
				Проводка.СуммаПРДт = 0;
				Проводка.СуммаВРДт = - СтрокаТаблицы.КорректировкаОбесценения;
			КонецЕсли;
		КонецЕсли;

		Если СвойстваСчетаОбесценения.НалоговыйУчет Тогда
			Проводка.СуммаНУКт = 0;
			Если Реквизиты.ПБУ18 Тогда
				Проводка.СуммаПРКт = 0;
				Проводка.СуммаВРКт = - СтрокаТаблицы.КорректировкаОбесценения;
			КонецЕсли;
		КонецЕсли;

	КонецЕсли;
	
КонецПроцедуры

Функция ВидСубконтоПоТипуОбъектаУчета(ТипОбъектаУчета)
	
	Если ТипОбъектаУчета = Тип("СправочникСсылка.ОсновныеСредства") Тогда
		Возврат ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.ОсновныеСредства;
	ИначеЕсли ТипОбъектаУчета = Тип("СправочникСсылка.НематериальныеАктивы") Тогда
		Возврат ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.НематериальныеАктивы;
	Иначе
		Возврат ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.ПустаяСсылка();
	КонецЕсли;
	
КонецФункции

Функция КредитОбесценения(ТипОбъектаУчета)
	
	Счет = ОсновнойСчетОбесцененияПоТипуОбъектаУчета(ТипОбъектаУчета);
	
	КредитОбесценения = Новый Структура;
	КредитОбесценения.Вставить("Счет",          Счет);
	КредитОбесценения.Вставить("СвойстваСчета", БухгалтерскийУчетВызовСервераПовтИсп.ПолучитьСвойстваСчета(Счет));
	
	Возврат КредитОбесценения;
	
КонецФункции

#КонецОбласти

#Область ПроводкиОбесценения

Функция ПроверитьРеквизитыУбытокОбесценения(ТаблицаРеквизиты)
	
	СписокОбязательныхКолонок = ""
		+ "Период,"                       // <Дата>
		+ "Организация,"                  // <СправочникСсылка.Организации>
		+ "СтатьяПрочихДоходовИРасходов," // <СправочникСсылка.ПрочиеДоходыИРасходы>
		+ "МежотчетныйПериод";            // <Булево>
	
	Реквизиты = Новый Структура(СписокОбязательныхКолонок);
	
	ОднострочнаяТаблица = ОбщегоНазначенияБПВызовСервера.ПолучитьТаблицуПараметровПроведения(
		ТаблицаРеквизиты,
		СписокОбязательныхКолонок);
	
	ЗаполнитьЗначенияСвойств(Реквизиты, ОднострочнаяТаблица[0]);
	
	Возврат Реквизиты;
	
КонецФункции

Функция ПроверитьУбытокОбесценения(ТаблицаОбесценение)
	
	СписокОбязательныхКолонок = ""
		+ "Объект,"               // <СправочникСсылка.ОсновныеСредства> или <СправочникСсылка.НематериальныеАктивы>
		+ "Подразделение,"        // <СправочникСсылка.ПодразделенияОрганизации>
		+ "ИзменениеОбесценения"; // <Число> - положительное для убытка
	
	Возврат ОбщегоНазначенияБПВызовСервера.ПолучитьТаблицуПараметровПроведения(ТаблицаОбесценение, СписокОбязательныхКолонок);
	
КонецФункции

Функция ДобавитьПроводкуУбытокОбесценения(Проводки, Запись, Реквизиты, СчетОбесценения, ВидСубконто, СодержаниеУбыток, СодержаниеВосстановление)

	Если Запись.ИзменениеОбесценения = 0 Тогда
		Возврат Неопределено;
	КонецЕсли;
	
	Проводка = Проводки.Добавить();
	
	Проводка.Период      = Реквизиты.Период;
	Проводка.Организация = Реквизиты.Организация;
	Если Реквизиты.МежотчетныйПериод Тогда
		Проводка.УточнениеПериода = БухгалтерскийУчетКлиентСервер.ОсновноеЗначениеУточненияМежотчетногоПериода();
	КонецЕсли;
	
	Если Запись.ИзменениеОбесценения > 0 Тогда
		
		Проводка.Сумма         = Запись.ИзменениеОбесценения;
		Проводка.Содержание    = СодержаниеУбыток;
		
		СчетУбытка             = ПланыСчетов.Хозрасчетный.ПрочиеРасходы;
		ИмяСчетУбытка          = "СчетДт";
		ИмяСубконтоУбытка      = "СубконтоДт";
		ИмяСчетОбесценения     = "СчетКт";
		ИмяСубконтоОбесценения = "СубконтоКт";
		
	Иначе
		
		Проводка.Сумма         = - Запись.ИзменениеОбесценения;
		Проводка.Содержание    = СодержаниеВосстановление;
		
		СчетУбытка             = ПланыСчетов.Хозрасчетный.ПрочиеДоходы;
		ИмяСчетУбытка          = "СчетКт";
		ИмяСубконтоУбытка      = "СубконтоКт";
		ИмяСчетОбесценения     = "СчетДт";
		ИмяСубконтоОбесценения = "СубконтоДт";
		
	КонецЕсли;
	
	Если Реквизиты.МежотчетныйПериод Тогда
		СчетУбытка = ПланыСчетов.Хозрасчетный.ПрибыльПодлежащаяРаспределению;
	КонецЕсли;
	
	Проводка[ИмяСчетУбытка] = СчетУбытка;
	БухгалтерскийУчет.УстановитьСубконто(
		Проводка[ИмяСчетУбытка],
		Проводка[ИмяСубконтоУбытка],
		ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.ПрочиеДоходыИРасходы,
		Реквизиты.СтатьяПрочихДоходовИРасходов);
		
	Проводка[ИмяСчетОбесценения] = СчетОбесценения;
	БухгалтерскийУчет.УстановитьСубконто(
		Проводка[ИмяСчетОбесценения],
		Проводка[ИмяСубконтоОбесценения],
		ВидСубконто,
		Запись.Объект);
	
	БухгалтерскийУчет.УстановитьПодразделенияПроводки(Проводка, Запись.Подразделение, Запись.Подразделение);
	
	Возврат Проводка;
	
КонецФункции

Функция ОсновнойСчетОбесцененияПоТипуОбъектаУчета(ТипОбъектаУчета)
	
	СчетаОбесценения = АмортизацияАктивов.СчетаОбесценения(ТипОбъектаУчета);
	Если Не ЗначениеЗаполнено(СчетаОбесценения) Тогда
		Возврат ПланыСчетов.Хозрасчетный.ПустаяСсылка();
	КонецЕсли;
	
	Возврат СчетаОбесценения[0];
	
КонецФункции

