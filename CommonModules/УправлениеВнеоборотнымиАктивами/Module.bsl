			Раздел1Текущий = Раздел1[НомерТекущейСтраницыРаздел1];
			СтраницаРаздел1 =  Раздел1Текущий.Данные[0].Значение;
			
			НомерБлокаРаздела1 = 0;
		КонецЕсли;
		
