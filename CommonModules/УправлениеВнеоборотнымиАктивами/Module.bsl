	|	НЕ ПодтверждающиеДокументы.ПрименяетсяКОтдельнымОбъектам
	|	И НЕ ПодтверждающиеДокументы.ПометкаУдаления
	|
	|ОБЪЕДИНИТЬ ВСЕ
	|
	|ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	ДанныеЛьгот.Организация,
	|	ДанныеЛьгот.ОсновноеСредство,
	|	ДанныеЛьгот.ОснованиеЛьготы,
	|	ПодтверждающиеДокументы.Ссылка.ПолноеНаименование,
	|	ПодтверждающиеДокументы.Ссылка.КемВыдан,
	|	ПодтверждающиеДокументы.Ссылка.Дата,
	|	ПодтверждающиеДокументы.Ссылка.Номер,
	|	ВЫБОР
	|		КОГДА ПодтверждающиеДокументы.Ссылка.СрокДействияОграничен
	|			ТОГДА 2
	|		ИНАЧЕ 1
	|	КОНЕЦ,
	|	ВЫБОР
	|		КОГДА ПодтверждающиеДокументы.Ссылка.СрокДействияОграничен
	|			ТОГДА ПодтверждающиеДокументы.Ссылка.НачалоДействия
	|		ИНАЧЕ ДАТАВРЕМЯ(1, 1, 1)
	|	КОНЕЦ,
	|	ВЫБОР
	|		КОГДА ПодтверждающиеДокументы.Ссылка.СрокДействияОграничен
	|			ТОГДА ПодтверждающиеДокументы.Ссылка.ОкончаниеДействия
	|		ИНАЧЕ ДАТАВРЕМЯ(1, 1, 1)
	|	КОНЕЦ
	|ИЗ
	|	ДанныеЛьгот КАК ДанныеЛьгот
	|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.ДокументыПодтверждающиеЛьготыПоИмущественнымНалогам.Объекты КАК
	|			ПодтверждающиеДокументы
	|		ПО ДанныеЛьгот.Организация = ПодтверждающиеДокументы.Ссылка.Организация
	|		И ДанныеЛьгот.ОсновноеСредство = ПодтверждающиеДокументы.ОсновноеСредство
	|		И ДанныеЛьгот.ОснованиеЛьготы = ПодтверждающиеДокументы.Ссылка.Владелец
	|ГДЕ
	|	ПодтверждающиеДокументы.Ссылка.ПрименяетсяКОтдельнымОбъектам
	|	И НЕ ПодтверждающиеДокументы.Ссылка.ПометкаУдаления
	|
	|УПОРЯДОЧИТЬ ПО
	|	Организация,
	|	ОсновноеСредство,
	|	ОснованиеЛьготы";
	
	РезультатЗапроса = Запрос.ВыполнитьПакет(); 
	
	ВыборкаОбъектов = РезультатЗапроса[2].Выбрать();
	ВыборкаДокументов = РезультатЗапроса[3].Выбрать();
	ОтборДокументов = Новый Структура("Организация, ОсновноеСредство, ОснованиеЛьготы");
	
	Пока ВыборкаОбъектов.Следующий() Цикл
		
		// Контейнер приходит с пустыми таблицами
		ДанныеСтраницыОбъекта = Контейнер.Земля.Добавить();
		ДанныеСтраницыОбъекта.УИД = Новый УникальныйИдентификатор;
				
		ДанныеСтраницыОбъекта.НомКадастрЗУ = ВыборкаОбъектов.КадастровыйНомер;
		ДанныеСтраницыОбъекта.ДатаНачСр = ВыборкаОбъектов.НачалоДействияЛьготы;
		ДанныеСтраницыОбъекта.ДатаКонСр = ВыборкаОбъектов.ОкончаниеДействияЛьготы;
		ДанныеСтраницыОбъекта.КодНалЛьготЗем = ВыборкаОбъектов.КодЛьготы;
		ДанныеСтраницыОбъекта.ДатаАкт = ВыборкаОбъектов.ДатаНормативногоАкта;
		ДанныеСтраницыОбъекта.НомерАкт = ВыборкаОбъектов.НомерНормативногоАкта;
		ДанныеСтраницыОбъекта.СтруктЕдАкт = ВыборкаОбъектов.СтруктурнаяЕдиница;
		
		ЗаполнитьЗначенияСвойств(ОтборДокументов, ВыборкаОбъектов);
		
		Пока ВыборкаДокументов.НайтиСледующий(ОтборДокументов) Цикл 
			ДанныеСтраницыДокумента = Контейнер.ЛьготаДокументЗемля.Добавить();
			ДанныеСтраницыДокумента.УИД = Новый УникальныйИдентификатор;
			ДанныеСтраницыДокумента.УИДРодителя = ДанныеСтраницыОбъекта.УИД;
			
			ДанныеСтраницыДокумента.НаимДок = ВыборкаДокументов.ПолноеНаименованиеДокумента;
			ДанныеСтраницыДокумента.ВыдДок = ВыборкаДокументов.КемВыдан;
			ДанныеСтраницыДокумента.ДатаДок = ВыборкаДокументов.Дата;
			ДанныеСтраницыДокумента.ПрПериодДок = ВыборкаДокументов.СрокДействияОграничен;
			ДанныеСтраницыДокумента.ДатаНачПериод = ВыборкаДокументов.НачалоДействия;
			ДанныеСтраницыДокумента.ДатаКонПериод = ВыборкаДокументов.ОкончаниеДействия;
			ДанныеСтраницыДокумента.НомерДок = ВыборкаДокументов.Номер;
		КонецЦикла
		
	КонецЦикла;
	
	#КонецОбласти
	
КонецПроцедуры

#КонецОбласти

#Область СообщениеОНаличииТранспортаИОбъектовПоКадастровойСтоимости

// Формирует список показателей регламентированного отчета.
//
// Параметры:
//  ПоказателиОтчета - Структура - структура, в которую добавляются показатели отчета.
//
Процедура ПолучитьСведенияОПоказателяхСообщенияОНаличииТранспортаИЗемли(ПоказателиОтчета) Экспорт
	
	РегламентированнаяОтчетность.ВставитьПоказательВСтруктуру(ПоказателиОтчета, "ВидТС", Истина, Ложь);
	РегламентированнаяОтчетность.ВставитьПоказательВСтруктуру(ПоказателиОтчета, "ИдНомТС", Истина, Ложь);
	РегламентированнаяОтчетность.ВставитьПоказательВСтруктуру(ПоказателиОтчета, "МаркаТС", Истина, Ложь);
	РегламентированнаяОтчетность.ВставитьПоказательВСтруктуру(ПоказателиОтчета, "РегЗнакТС", Истина, Ложь);
	РегламентированнаяОтчетность.ВставитьПоказательВСтруктуру(ПоказателиОтчета, "ДатаРегТС", Истина, Ложь);
	РегламентированнаяОтчетность.ВставитьПоказательВСтруктуру(ПоказателиОтчета, "ДатаСнУчетТС", Истина, Ложь);
	
	РегламентированнаяОтчетность.ВставитьПоказательВСтруктуру(ПоказателиОтчета, "НомКадастрЗУ", Истина, Ложь);
	
	РегламентированнаяОтчетность.ВставитьПоказательВСтруктуру(ПоказателиОтчета, "ДатаРегПрав", Истина, Ложь);
	РегламентированнаяОтчетность.ВставитьПоказательВСтруктуру(ПоказателиОтчета, "ДатаПрекрПрав", Истина, Ложь);
	РегламентированнаяОтчетность.ВставитьПоказательВСтруктуру(ПоказателиОтчета, "Комментарий", Истина, Ложь);
	
КонецПроцедуры

// Заполняет показатели регламентированного отчета.
//
// Параметры:
//  ПараметрыОтчета - Структура - параметры для заполнения отчета.
//  Контейнер - Структура - структура, в которую помещаются заполненные показатели отчета.
//
Процедура ЗаполнитьСообщениеОНаличииТранспортаИЗемли(ПараметрыОтчета, Контейнер) Экспорт
	
	КонецПоследнегоНалоговогоПериода = 
		УправлениеВнеоборотнымиАктивамиВызовСервера.ПоследнийОтчетныйПериодПоДатеУведомления(ПараметрыОтчета.ДатаПодписи);
	КодИФНС = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(ПараметрыОтчета.РегистрацияВИФНС, "Код");
	
	Запрос = Новый Запрос;
	
	Запрос.УстановитьПараметр("Организации", ОбщегоНазначенияБПВызовСервераПовтИсп.ВсяОрганизация(ПараметрыОтчета.Организация));
	Запрос.УстановитьПараметр("ПериодРасчета", КонецПоследнегоНалоговогоПериода);
	
	#Область ТранспортныеСредства

	ДокументСверкаСФНС = Документы.СверкаСФНСПоИмущественнымНалогам.НайтиСуществующий(
		ПараметрыОтчета.Организация,
		Перечисления.ВидыИмущественныхНалогов.ТранспортныйНалог,
		КонецПоследнегоНалоговогоПериода,
		КодИФНС);
		
	Если ЗначениеЗаполнено(ДокументСверкаСФНС) Тогда
		
		Запрос.Текст = 
		"ВЫБРАТЬ РАЗЛИЧНЫЕ
		|	СверкаСФНСПоИмущественнымНалогамСверка.ОсновноеСредство КАК ОсновноеСредство
		|ПОМЕСТИТЬ ДанныеСверки
		|ИЗ
		|	Документ.СверкаСФНСПоИмущественнымНалогам.Сверка КАК СверкаСФНСПоИмущественнымНалогамСверка
		|ГДЕ
		|	СверкаСФНСПоИмущественнымНалогамСверка.Ссылка = &ДокументСверкаСФНС
		|	И СверкаСФНСПоИмущественнымНалогамСверка.Действие = ЗНАЧЕНИЕ(Перечисление.ДействияСверкиИмущественныхНалогов.ОтправитьСообщениеОНаличииОбъектов)
		|
		|ИНДЕКСИРОВАТЬ ПО
		|	ОсновноеСредство
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ РАЗЛИЧНЫЕ
		|	РасчетТранспортногоНалога.ОсновноеСредство КАК ОсновноеСредство,
		|	РасчетТранспортногоНалога.ВидТранспортногоСредства КАК ВидТранспортногоСредства,
		|	РасчетТранспортногоНалога.ИдентификационныйНомер КАК ИдНомТС,
		|	РасчетТранспортногоНалога.Марка КАК МаркаТС,
		|	РасчетТранспортногоНалога.РегистрационныйЗнак КАК РегЗнакТС,
		|	РасчетТранспортногоНалога.ДатаРегистрации КАК ДатаРегТС,
		|	РасчетТранспортногоНалога.ДатаСнятияСУчета КАК ДатаСнУчетТС
		|ИЗ
		|	РегистрСведений.РасчетТранспортногоНалога КАК РасчетТранспортногоНалога
		|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ДанныеСверки КАК ДанныеСверки
		|		ПО РасчетТранспортногоНалога.ОсновноеСредство = ДанныеСверки.ОсновноеСредство
		|ГДЕ
		|	РасчетТранспортногоНалога.Организация В(&Организации)
		|	И РасчетТранспортногоНалога.ПериодРасчета = &ПериодРасчета
		|
		|УПОРЯДОЧИТЬ ПО
		|	ОсновноеСредство,
		|	ДатаРегТС";
		
		Запрос.УстановитьПараметр("ДокументСверкаСФНС", ДокументСверкаСФНС);
		
		ВыборкаТС = Запрос.Выполнить().Выбрать();
		
		СведенияОВидахТранспортныхСредств = РасчетИмущественныхНалоговВызовСервера.СведенияОВидахТранспортныхСредств();
		
		Пока ВыборкаТС.Следующий() Цикл
			
			// Контейнер приходит с пустой таблицей раздела Транспорт 
			ДанныеСтраницы = Контейнер.Транспорт.Добавить();
			ДанныеСтраницы.УИД = Новый УникальныйИдентификатор;
			
			ЗаполнитьЗначенияСвойств(ДанныеСтраницы, ВыборкаТС);
			
			ДанныеСтраницы.ВидТС = РасчетИмущественныхНалоговВызовСервера.НомерКатегорииПоВидуТранспортногоСредства(
				ВыборкаТС.ВидТранспортногоСредства,
				СведенияОВидахТранспортныхСредств);
			
		КонецЦикла;
		
	КонецЕсли;
	
	#КонецОбласти
	
	#Область ЗемельныеУчастки
	
	ДокументСверкаСФНС = Документы.СверкаСФНСПоИмущественнымНалогам.НайтиСуществующий(
		ПараметрыОтчета.Организация,
		Перечисления.ВидыИмущественныхНалогов.ЗемельныйНалог,
		КонецПоследнегоНалоговогоПериода,
		КодИФНС);
		
	Если ЗначениеЗаполнено(ДокументСверкаСФНС) Тогда
		
		Запрос.Текст = 
		"ВЫБРАТЬ РАЗЛИЧНЫЕ
		|	СверкаСФНСПоИмущественнымНалогамСверка.ОсновноеСредство КАК ОсновноеСредство
		|ПОМЕСТИТЬ ДанныеСверки
		|ИЗ
		|	Документ.СверкаСФНСПоИмущественнымНалогам.Сверка КАК СверкаСФНСПоИмущественнымНалогамСверка
		|ГДЕ
		|	СверкаСФНСПоИмущественнымНалогамСверка.Ссылка = &ДокументСверкаСФНС
		|	И СверкаСФНСПоИмущественнымНалогамСверка.Действие = ЗНАЧЕНИЕ(Перечисление.ДействияСверкиИмущественныхНалогов.ОтправитьСообщениеОНаличииОбъектов)
		|
		|ИНДЕКСИРОВАТЬ ПО
		|	ОсновноеСредство
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ РАЗЛИЧНЫЕ
		|	РасчетЗемельногоНалога.КадастровыйНомер КАК НомКадастрЗУ,
		|	РасчетЗемельногоНалога.ОсновноеСредство.Наименование КАК Комментарий,
		|	РасчетЗемельногоНалога.ДатаРегистрации КАК ДатаРегПрав,
		|	РасчетЗемельногоНалога.ДатаСнятияСУчета КАК ДатаПрекрПрав
		|ИЗ
		|	РегистрСведений.РасчетЗемельногоНалога КАК РасчетЗемельногоНалога
		|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ДанныеСверки КАК ДанныеСверки
		|		ПО РасчетЗемельногоНалога.ОсновноеСредство = ДанныеСверки.ОсновноеСредство
		|ГДЕ
		|	РасчетЗемельногоНалога.Организация В(&Организации)
		|	И РасчетЗемельногоНалога.ПериодРасчета = &ПериодРасчета";
		
		Запрос.УстановитьПараметр("ДокументСверкаСФНС", ДокументСверкаСФНС);
		
		ВыборкаЗУ = Запрос.Выполнить().Выбрать();
			
		Пока ВыборкаЗУ.Следующий() Цикл
			
			// Контейнер приходит с пустой таблицей раздела Земля 
			ДанныеСтраницы = Контейнер.Земля.Добавить();
			ДанныеСтраницы.УИД = Новый УникальныйИдентификатор;
				
			ЗаполнитьЗначенияСвойств(ДанныеСтраницы, ВыборкаЗУ);
			
		КонецЦикла;
		
	КонецЕсли;
	
	#КонецОбласти

	#Область ИмуществоПоКадастровойСтоимости
	
	ДокументСверкаСФНС = Документы.СверкаСФНСПоИмущественнымНалогам.НайтиСуществующий(
		ПараметрыОтчета.Организация,
		Перечисления.ВидыИмущественныхНалогов.НалогНаИмущество,
		КонецПоследнегоНалоговогоПериода,
		КодИФНС);
		
	Если ЗначениеЗаполнено(ДокументСверкаСФНС) Тогда
		
		Запрос.Текст = 
		"ВЫБРАТЬ РАЗЛИЧНЫЕ
		|	СверкаСФНСПоИмущественнымНалогамСверка.КадастровыйНомер КАК КадастровыйНомер,
		|	СверкаСФНСПоИмущественнымНалогамСверка.ОсновноеСредство.Наименование КАК Комментарий
		|ИЗ
		|	Документ.СверкаСФНСПоИмущественнымНалогам.Сверка КАК СверкаСФНСПоИмущественнымНалогамСверка
		|ГДЕ
		|	СверкаСФНСПоИмущественнымНалогамСверка.Ссылка = &ДокументСверкаСФНС
		|	И СверкаСФНСПоИмущественнымНалогамСверка.Действие = ЗНАЧЕНИЕ(Перечисление.ДействияСверкиИмущественныхНалогов.ОтправитьСообщениеОНаличииОбъектов)";
		
		Запрос.УстановитьПараметр("ДокументСверкаСФНС", ДокументСверкаСФНС);
		
		ВыборкаИмущество = Запрос.Выполнить().Выбрать();
			
		Пока ВыборкаИмущество.Следующий() Цикл
			
			// В раздел Земля выше уже могли быть добавлены участки.
			// Имущество добавляется в этот же раздел.
			ДанныеСтраницы = Контейнер.Земля.Добавить();
			ДанныеСтраницы.УИД = Новый УникальныйИдентификатор;
			
			ДанныеСтраницы.НомКадастрЗУ = ВыборкаИмущество.КадастровыйНомер;
			ДанныеСтраницы.Комментарий = ВыборкаИмущество.Комментарий;
			
		КонецЦикла;
		
	КонецЕсли;
	
	#КонецОбласти
	
КонецПроцедуры

#КонецОбласти

#Область УведомлениеОГибелиТС

// Формирует список показателей регламентированного отчета.
//
// Параметры:
//  ПоказателиОтчета - Структура - структура, в которую добавляются показатели отчета.
//
Процедура ПолучитьСведенияОПоказателяхУведомленияОГибелиТС(ПоказателиОтчета) Экспорт
	
	РегламентированнаяОтчетность.ВставитьПоказательВСтруктуру(ПоказателиОтчета, "ВидТС", Истина, Ложь);
	РегламентированнаяОтчетность.ВставитьПоказательВСтруктуру(ПоказателиОтчета, "МаркаТС", Истина, Ложь);
	РегламентированнаяОтчетность.ВставитьПоказательВСтруктуру(ПоказателиОтчета, "РегНомерТС", Истина, Ложь);
	РегламентированнаяОтчетность.ВставитьПоказательВСтруктуру(ПоказателиОтчета, "ДатаГибелиТС", Истина, Ложь);
		
КонецПроцедуры

// Заполняет показатели регламентированного отчета.
//
// Параметры:
//  ПараметрыОтчета - Структура - параметры для заполнения отчета.
//  Контейнер - Структура - структура, в которую помещаются заполненные показатели отчета.
//
Процедура ЗаполнитьУведомлениеОГибелиТС(ПараметрыОтчета, Контейнер) Экспорт
	
	КонецПоследнегоНалоговогоПериода = 
		УправлениеВнеоборотнымиАктивамиВызовСервера.ПоследнийОтчетныйПериодПоДатеУведомления(ПараметрыОтчета.ДатаПодписи);
	КодИФНС = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(ПараметрыОтчета.РегистрацияВИФНС, "Код");
		
	ДокументСверкаСФНС = Документы.СверкаСФНСПоИмущественнымНалогам.НайтиСуществующий(
		ПараметрыОтчета.Организация,
		Перечисления.ВидыИмущественныхНалогов.ТранспортныйНалог,
		КонецПоследнегоНалоговогоПериода,
		КодИФНС);
	
	Запрос = Новый Запрос;
	
	Если ЗначениеЗаполнено(ДокументСверкаСФНС) Тогда
		
		// Только те ТС из сверки, по которым указано, что в ФНС нет данных об их гибели/уничтожении
		Запрос.Текст = 
		"ВЫБРАТЬ РАЗЛИЧНЫЕ
		|	СверкаСФНСПоИмущественнымНалогамСверка.ОсновноеСредство КАК ОсновноеСредство
		|ПОМЕСТИТЬ ДанныеСверки
		|ИЗ
		|	Документ.СверкаСФНСПоИмущественнымНалогам.Сверка КАК СверкаСФНСПоИмущественнымНалогамСверка
		|ГДЕ
		|	СверкаСФНСПоИмущественнымНалогамСверка.Ссылка = &ДокументСверкаСФНС
		|	И СверкаСФНСПоИмущественнымНалогамСверка.Действие = ЗНАЧЕНИЕ(Перечисление.ДействияСверкиИмущественныхНалогов.ОтправитьЗаявлениеОГибелиТС)
		|
		|ИНДЕКСИРОВАТЬ ПО
		|	ОсновноеСредство
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ
		|	РегистрацияТранспортныхСредств.Организация КАК Организация,
		|	РегистрацияТранспортныхСредств.ОсновноеСредство КАК ОсновноеСредство,
		|	МАКСИМУМ(РегистрацияТранспортныхСредств.Период) КАК ДатаРегистрации
		|ПОМЕСТИТЬ ДатыРегистрацииТС
		|ИЗ
		|	РегистрСведений.РегистрацияТранспортныхСредств КАК РегистрацияТранспортныхСредств
		|ГДЕ
		|	РегистрацияТранспортныхСредств.Организация В(&Организации)
		|	И РегистрацияТранспортныхСредств.ОсновноеСредство В
		|			(ВЫБРАТЬ
		|				ДанныеСверки.ОсновноеСредство
		|			ИЗ
		|				ДанныеСверки КАК ДанныеСверки)
		|	И РегистрацияТранспортныхСредств.Период <= &КонецПериода
		|	И РегистрацияТранспортныхСредств.ВключатьВНалоговуюБазу
		|
		|СГРУППИРОВАТЬ ПО
		|	РегистрацияТранспортныхСредств.Организация,
		|	РегистрацияТранспортныхСредств.ОсновноеСредство
		|
		|ИНДЕКСИРОВАТЬ ПО
		|	Организация,
		|	ОсновноеСредство
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ
		|	РегистрацияТранспортныхСредствСрезПоследних.ОсновноеСредство КАК ОсновноеСредство,
		|	МАКСИМУМ(РегистрацияТранспортныхСредствСрезПоследних.Период) КАК ДатаГибели
		|ПОМЕСТИТЬ ДатыГибелиТС
		|ИЗ
		|	РегистрСведений.РегистрацияТранспортныхСредств.СрезПоследних(
		|			&КонецПериода,
		|			Организация В (&Организации)
		|				И ОсновноеСредство В
		|					(ВЫБРАТЬ
		|						ДанныеСверки.ОсновноеСредство
		|					ИЗ
		|						ДанныеСверки КАК ДанныеСверки)) КАК РегистрацияТранспортныхСредствСрезПоследних
		|ГДЕ
		|	НЕ РегистрацияТранспортныхСредствСрезПоследних.ВключатьВНалоговуюБазу
		|
		|СГРУППИРОВАТЬ ПО
		|	РегистрацияТранспортныхСредствСрезПоследних.ОсновноеСредство
		|
		|ИНДЕКСИРОВАТЬ ПО
		|	ОсновноеСредство
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ РАЗЛИЧНЫЕ
		|	ДатыРегистрацииТС.ОсновноеСредство КАК ОсновноеСредство,
		|	РегистрацияТранспортныхСредств.ВидТранспортногоСредства КАК ВидТранспортногоСредства,
		|	РегистрацияТранспортныхСредств.Марка КАК Марка,
		|	РегистрацияТранспортныхСредств.РегистрационныйЗнак КАК РегистрационныйЗнак
		|ПОМЕСТИТЬ ДанныеРегистрацииПогибшихТС
		|ИЗ
		|	ДатыРегистрацииТС КАК ДатыРегистрацииТС
		|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.РегистрацияТранспортныхСредств КАК РегистрацияТранспортныхСредств
		|		ПО ДатыРегистрацииТС.Организация = РегистрацияТранспортныхСредств.Организация
		|			И ДатыРегистрацииТС.ОсновноеСредство = РегистрацияТранспортныхСредств.ОсновноеСредство
		|			И ДатыРегистрацииТС.ДатаРегистрации = РегистрацияТранспортныхСредств.Период
		|ГДЕ
		|	РегистрацияТранспортныхСредств.ВключатьВНалоговуюБазу
		|
		|ИНДЕКСИРОВАТЬ ПО
		|	ОсновноеСредство
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ
		|	ДанныеСверки.ОсновноеСредство КАК ОсновноеСредство,
		|	ЕСТЬNULL(ДанныеРегистрацииПогибшихТС.ВидТранспортногоСредства, НЕОПРЕДЕЛЕНО) КАК ВидТранспортногоСредства,
		|	ЕСТЬNULL(ДанныеРегистрацииПогибшихТС.Марка, """") КАК МаркаТС,
		|	ЕСТЬNULL(ДанныеРегистрацииПогибшихТС.РегистрационныйЗнак, """") КАК РегНомерТС,
		|	ЕСТЬNULL(ДатыГибелиТС.ДатаГибели, ДАТАВРЕМЯ(1, 1, 1)) КАК ДатаГибелиТС
		|ИЗ
		|	ДанныеСверки КАК ДанныеСверки
		|		ЛЕВОЕ СОЕДИНЕНИЕ ДанныеРегистрацииПогибшихТС КАК ДанныеРегистрацииПогибшихТС
		|		ПО ДанныеСверки.ОсновноеСредство = ДанныеРегистрацииПогибшихТС.ОсновноеСредство
		|		ЛЕВОЕ СОЕДИНЕНИЕ ДатыГибелиТС КАК ДатыГибелиТС
		|		ПО ДанныеСверки.ОсновноеСредство = ДатыГибелиТС.ОсновноеСредство";
		
		Запрос.УстановитьПараметр("ДокументСверкаСФНС", ДокументСверкаСФНС);
		Запрос.УстановитьПараметр("КонецПериода", КонецПоследнегоНалоговогоПериода);
		
	Иначе
		
		// Все ТС, когда-либо снятые с учета
		Запрос.Текст = 
		"ВЫБРАТЬ РАЗЛИЧНЫЕ
		|	РегистрацияТССрезПоследних.ОсновноеСредство КАК ОсновноеСредство,
		|	РегистрацияТССрезПоследних.ВидТранспортногоСредства КАК ВидТранспортногоСредства,
		|	РегистрацияТССрезПоследних.Марка КАК МаркаТС,
		|	РегистрацияТССрезПоследних.РегистрационныйЗнак КАК РегНомерТС,
		|	РегистрацияТССрезПоследних.Период КАК ДатаГибелиТС
		|ИЗ
		|	РегистрСведений.РегистрацияТранспортныхСредств.СрезПоследних(&Период, Организация В (&Организации)) КАК РегистрацияТССрезПоследних
		|ГДЕ
		|	НЕ РегистрацияТССрезПоследних.ВключатьВНалоговуюБазу";
		
		Запрос.УстановитьПараметр("Период", 
			?(ЗначениеЗаполнено(ПараметрыОтчета.ДатаПодписи), ПараметрыОтчета.ДатаПодписи, ОбщегоНазначения.ТекущаяДатаПользователя()));
		
	КонецЕсли;
	
	Запрос.УстановитьПараметр("Организации", ОбщегоНазначенияБПВызовСервераПовтИсп.ВсяОрганизация(ПараметрыОтчета.Организация));
	
	ВыборкаТС = Запрос.Выполнить().Выбрать();
	
	СведенияОВидахТранспортныхСредств = РасчетИмущественныхНалоговВызовСервера.СведенияОВидахТранспортныхСредств();
	
	Пока ВыборкаТС.Следующий() Цикл
		
		// Контейнер приходит с пустой таблицей раздела Транспорт 
		ДанныеСтраницы = Контейнер.Транспорт.Добавить();
		ДанныеСтраницы.УИД = Новый УникальныйИдентификатор;
		
		ЗаполнитьЗначенияСвойств(ДанныеСтраницы, ВыборкаТС);
		
		ДанныеСтраницы.ВидТС = РасчетИмущественныхНалоговВызовСервера.НомерКатегорииПоВидуТранспортногоСредства(
			ВыборкаТС.ВидТранспортногоСредства,
			СведенияОВидахТранспортныхСредств);
		
	КонецЦикла;

КонецПроцедуры

