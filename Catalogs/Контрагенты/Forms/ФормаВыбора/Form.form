      <fields xsi:type="schema:DataCompositionSchemaDataSetField">
        <dataPath>УдалитьОсновнойДоговорКонтрагента</dataPath>
        <field>УдалитьОсновнойДоговорКонтрагента</field>
      </fields>
      <fields xsi:type="schema:DataCompositionSchemaDataSetField">
        <dataPath>ПроверкаКонтрагентовКонтрагентНеСуществует</dataPath>
        <field>ПроверкаКонтрагентовКонтрагентНеСуществует</field>
        <title>
          <localValue>
